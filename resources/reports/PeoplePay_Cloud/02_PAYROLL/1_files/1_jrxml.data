		</parameter>
		<queryString>
			<![CDATA[select a.percent_split, c.net_pay, to_char(a.percent_split * c.net_pay, '999,999,999') disb_pay,a.payment_mode_dist,c.payment_mode_hist
from hr_rcm_disbursement a
join hr_rcm_salary b on b.id=a.salary_id
join pa_pmg_payrolldtl c on c.emp_id=b.emp_id
where c.id=$P{PAYSLIP_NO}
and sysdate between b.start_date and nvl(b.end_date,sysdate)]]>
