	<queryString>
		<![CDATA[select distinct
SALARY_CURRENCY,
pa.org_id,
pa.pay_batch_id,ep.emp_company_no emp_id,
pa.id pay_slip,
nvl(pa.surname_hist,r.surname)||', '||nvl(pa.first_name_hist,r.first_name)||' '||nvl(r.middle_name,'') employee_name,
nvl(pa.orgdtl_id,fn_getWorkLocation(pa.org_id, pa.wklocation_id)) WORK_LOCATION_ID,
nvl(v.unit_name,fn_getDepartment(pa.org_id, pa.position_id, pa.wklocation_id)) WORK_LOCATION,
nvl(pa.department_name_hist,'Unknown')department_name_hist,pa.surname_hist,pa.first_name_hist,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.total_deduction else TOTAL_DEDUCTION_CONV end total_deduction,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.gross_taxable+pa.gross_nontaxable else GROSS_PAY_CONV end  AS gross,
case when pa.org_id=18658 and payment_type_hist='WEEKLY' then 
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.hourly_rate*200 else round(pkg_global_fnts.fn_curr_convert(pa.org_id, pa.hourly_rate*200, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end else 
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then basic_personal_rate else round(pkg_global_fnts.fn_curr_convert(pa.org_id,basic_personal_rate, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end  
end basic_personal_rate,
nvl(case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.HOURLY_RATE else round(pkg_global_fnts.fn_curr_convert(pa.org_id,pa.HOURLY_RATE, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end,0) hourly_rate,
case when pa.time_tax_earnings = 0 then 
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then basic_personal_rate else round(pkg_global_fnts.fn_curr_convert(pa.org_id,basic_personal_rate, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end  
else 
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.time_tax_earnings else round(pkg_global_fnts.fn_curr_convert(pa.org_id,pa.time_tax_earnings, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end end time_tax_earnings,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.nis_employee else round(pkg_global_fnts.fn_curr_convert(pa.org_id,pa.nis_employee, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end nis_employee,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then case when pa.tax_amount<0 then 0 else pa.tax_amount end  else round(pkg_global_fnts.fn_curr_convert(pa.org_id,pa.tax_amount, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end tax_amount,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.net_pay else NET_PAY_CONV end net_pay,
pa.organisation_name,
ho.address,ho.faxnum,ho.telnum,nis_reg_no,tax_reg_no,
pa.period_start,pa.period_end,
pa.payroll_effective,employment_class_hist,payment_type_hist,
initcap(employment_class_hist|| ' '||payment_type_hist||' '||initcap(replace(compute_gross_hist,'_',' '))||' '||pa.period_start||' -- '||pa.period_end) as payroll_description,
upper(pa.payment_mode_hist) payment_mode_hist, nvl(v.coa_code,'None') coa_code
from pa_pmg_payrolldtl pa
JOIN PA_PMG_PAYROLLHD PH ON PA.PAY_BATCH_ID=PH.ID
join hr_rcm_employee ep on pa.emp_id=ep.id
join HR_RCM_INDIVIDUAL r on ep.ind_id=r.id
join hr_hcf_organisation ho on pa.org_id=ho.id
left outer join HR_HCF_ORGSTRUCTUREDTL v on pa.orgdtl_id=v.id
left join hr_rcm_salary s on s.id=pa.salary_id
WHERE 
((upper(pa.PAYMENT_TYPE_HIST) = case   when $P{EARNINGS_PERIOD_ID}=-25 then to_char(upper(pa.PAYMENT_TYPE_HIST))	                               
                                    when $P{EARNINGS_PERIOD_ID}=-15  then 'MONTHLY'
                                    when $P{EARNINGS_PERIOD_ID}=-5    then 'WEEKLY'
                                    end
and extract(year from pa.period_end)||extract(month from pa.period_end)=  $P{PERIOD_MONTH_YEAR} 
and pa.org_id= $P{IN_ORG_ID} 
)
or 
pa.EARNINGS_PERIOD_ID=  $P{EARNINGS_PERIOD_ID} 
) 

and 
(  $P{allEmployees}  = 1 or 
exists (select 1
                        from RPT_PARAMETER w1
                        WHERE 
                        W1.SESSION_ID=  $P{SESSION_ID} 
                         and
                         option_value=case when option_name =   $P{SEARCH_OPTION}  and option_name ='EMPLOYEE' then to_char(ep.id)                                               
                                                when option_name =  $P{SEARCH_OPTION}   and option_name ='POSITION' then to_char(ep.position_id)
                                                when option_name =  $P{SEARCH_OPTION}   and option_name ='WORK_LOCATION' then to_char(ep.wklocation_id)
                                                when option_name =  $P{SEARCH_OPTION}   and option_name ='UNION' then to_char(ep.union_code_id)
                                                when option_name =  $P{SEARCH_OPTION}   and option_name ='EMPLOYMENT_TYPE' then to_char(ep.EMPLOYMENT_TYPE_ID)
                                                when option_name =  $P{SEARCH_OPTION}   and option_name ='ORG_STRUCT_LEVEL' then to_char(v.id)
                                                end
                        )

)]]>
	</queryString>
