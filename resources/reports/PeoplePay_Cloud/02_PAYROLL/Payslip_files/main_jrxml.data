		<queryString>
			<![CDATA[select description,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then round(pd.amount_deducted) else round(pkg_global_fnts.fn_curr_convert(pa.org_id,pd.amount_deducted, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end amount_deducted,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then round(pd.amount_deducted_ytd) else round(pkg_global_fnts.fn_curr_convert(pa.org_id,pd.amount_deducted_ytd, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end amount_deducted_ytd
	from PA_PMG_PAYROLLDEDUCTION pd 
    join PA_PCF_ADMINDEDUCTION adm ON adm.id = pd.adm_deduction_id
	join PA_PCF_DEDUCTIONCODE ded ON adm.expense_id = ded.id
    join   pa_pmg_payrolldtl pa on pa.id=pd.pay_slipno
    left join hr_rcm_salary s on s.id=pa.salary_id
	where
	 (upper(ded.Who_Pays) ='EMPLOYEE' or upper(adm.Deduction_Type)='EMPLOYEE')
    and ded.org_id=$P{P_ORG_ID}
    and pd.pay_slipno=$P{P_PAY_SLIP_ID}]]>
		</queryString>
		<field name="DESCRIPTION" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="DESCRIPTION"/>
		</field>
		<field name="AMOUNT_DEDUCTED" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="AMOUNT_DEDUCTED"/>
		</field>
