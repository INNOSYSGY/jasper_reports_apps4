	<queryString>
		<![CDATA[select
SALARY_CURRENCY,
pa.org_id,
pa.pay_batch_id,
to_char(date_employed,'yyyy-mm-dd') doe,
pa.id pay_slip_no,pa.surname_hist,pa.first_name_hist,pa.salary_id,pa.period_start,pa.period_end,
pa.payroll_effective,pa.department_name_hist,pa.organisation_name,initcap(PA.PAYMENT_TYPE_HIST)PAYMENT_TYPE_HIST,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.basic_personal_rate else round(pkg_global_fnts.fn_curr_convert(PA.ORG_ID,pa.basic_personal_rate, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end basic_personal_rate,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.basic_personal_rate_ytd else round(pkg_global_fnts.fn_curr_convert(PA.ORG_ID,pa.basic_personal_rate_ytd, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end basic_personal_rate_ytd,
pa.org_id,ep.emp_company_no emp_id,pa.id pay_slip,
NVL(PA.SURNAME_HIST,'')||', '||NVL(PA.FIRST_NAME_HIST,'')||' '||NVL(R.MIDDLE_NAME,'') EMPLOYEE_NAME,
nvl(v.unit_name,'Unknown') WORK_LOCATION,
pa.POSITION_NAME_HIST position_name_hist ,pa.surname_hist,pa.first_name_hist,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.total_deduction else TOTAL_DEDUCTION_CONV end total_deduction,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.gross_taxable+pa.gross_nontaxable else GROSS_PAY_CONV end  AS gross,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.time_tax_earnings else round(pkg_global_fnts.fn_curr_convert(PA.ORG_ID,pa.time_tax_earnings, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end time_tax_earnings,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.nis_employee else round(pkg_global_fnts.fn_curr_convert(PA.ORG_ID,pa.nis_employee, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end nis_employee,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then case when pa.tax_amount<0 then 0 else pa.tax_amount end else round(pkg_global_fnts.fn_curr_convert(PA.ORG_ID,pa.tax_amount, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end tax_amount,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.net_pay else NET_PAY_CONV end net_pay,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.gross_taxable_ytd+pa.gross_nontaxable_ytd else round(pkg_global_fnts.fn_curr_convert(PA.ORG_ID,pa.gross_taxable_ytd+pa.gross_nontaxable_ytd, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end  gross_ytd,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.nis_employee_ytd else round(pkg_global_fnts.fn_curr_convert(PA.ORG_ID,pa.nis_employee_ytd, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end nis_employee_ytd,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then time_tax_earnings_ytd else round(pkg_global_fnts.fn_curr_convert(PA.ORG_ID,time_tax_earnings_ytd, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end time_tax_earnings_ytd,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.net_pay_ytd else round(pkg_global_fnts.fn_curr_convert(PA.ORG_ID,pa.net_pay_ytd, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end net_pay_ytd,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then pa.total_deduction_ytd else round(pkg_global_fnts.fn_curr_convert(PA.ORG_ID,pa.total_deduction_ytd, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end total_deduction_ytd,
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then case when pa.tax_amount_ytd<0 then 0 else pa.tax_amount_ytd end else round(pkg_global_fnts.fn_curr_convert(PA.ORG_ID,pa.tax_amount_ytd, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end tax_amount_ytd,
(case when manager_approved_by is null then 'N' else 'Y' end) approved,
Pkg_Global_Fnts.Fn_Getcurnistaxno(r.Id , 'TIN',  pa.PERIOD_START ) Tax_no,
nvl('- '||P.POSITION_NAME, '') position_name,
case when (select nvl(sum(1),0) from 
PA_PMG_PAYROLLDEDUCTION pd where pd.PAY_SLIPNO=pa.id
and EXPENSE_CODE in ('66','85','84','77')
)>=1 then 'MMG: '||nvl(cast(r.CELL_PHONE as varchar2(100)),' ')  else ' ' end MMG
from pa_pmg_payrolldtl pa
join PA_PMG_PAYROLLHD ph on pa.pay_batch_id=ph.id
join HR_RCM_EMPLOYEE ep on pa.emp_id=ep.id
join HR_RCM_INDIVIDUAL r on ep.ind_id=r.id
join hr_hcf_organisation ho ON PA.ORG_ID=HO.ID
left join hr_rcm_salary s on s.id=pa.salary_id
left join HR_HCF_POSITION P on P.id=ep.position_id
left outer join HR_HCF_ORGSTRUCTUREDTL v on pa.orgdtl_id=v.id
WHERE PAY_BATCH_ID=$P{ID}
and pa.id=case when  $P{PAYSLIP_ID}  is null then pa.id else  $P{PAYSLIP_ID}  end
order by nvl(v.unit_name,'Unknown'),pa.surname_hist,pa.first_name_hist]]>
	</queryString>
