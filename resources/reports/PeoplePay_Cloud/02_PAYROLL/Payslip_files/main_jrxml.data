		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="P_PAY_SLIP_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="P_ORG_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT 
O.PAY_SLIPNO,  
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then round(O.INCOME_AMOUNT) else round(pkg_global_fnts.fn_curr_convert(pa.org_id, O.INCOME_AMOUNT, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end INCOME_AMOUNT, 
case when SALARY_CURRENCY='GYD' or SALARY_CURRENCY is null then round(INCOME_AMOUNT_YTD) else round(pkg_global_fnts.fn_curr_convert(pa.org_id,INCOME_AMOUNT_YTD, 'NO_TO_BASE',  'GYD',  SALARY_CURRENCY),2) end INCOME_AMOUNT_YTD,
(CASE  WHEN trim(ic.USE_STANDARD_HOURS) = 'Y' and O.income_code='90' THEN 
        ic.Description||'('||quantity||' Days)'
        WHEN trim(ic.USE_STANDARD_HOURS) = 'Y' and O.income_code!='90' THEN 
        ic.Description||'('||quantity||' HR) ' ||'at (Rate:'||REPLACE(TO_CHAR(o.rate,'$999,999'),' ','')||')'
        WHEN ic.admin_status = 'MAN' and o.quantity >= 1 then 
        ic.Description||'('||o.quantity||' HR) ' ||'at (Rate:'||REPLACE(TO_CHAR(o.rate,'$999,999'),' ','')||')'
        ELSE ic.Description END)Description, o.Income_code, O.Emp_id
FROM   PA_PMG_PAYROLLINCOME O 
join   pa_pmg_payrolldtl pa on pa.id=o.pay_slipno
left join hr_rcm_salary s on s.id=pa.salary_id
INNER JOIN PA_PCF_INCOMECODE IC ON O.INCOME_CODE = IC.INCOME_CODE
where	pay_slipno=$P{P_PAY_SLIP_ID}
and IC.org_id=$P{P_ORG_ID}
	and Income_Amount>0]]>
		</queryString>
