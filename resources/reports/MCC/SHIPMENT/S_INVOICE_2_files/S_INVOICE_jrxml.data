	<subDataset name="Shipment_Penalty" uuid="62ae6be4-bcb8-4842-9fb0-8afd556f95c5">
		<parameter name="p_invoice_id" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="p_transaction_type" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
pkg_mcc.f_return_elements (ELEMENTS) Elements,
PENALTY_COST_PER_UNIT,
DEDUCT_FOR_EACH,
DEDUCT_CONDITION_VALUE,
case
when QUANTITY_MNT_UNT = '%' then QUANTITY_MNT_UNT
else ' '||QUANTITY_MNT_UNT end QUANTITY_MNT_UNT,
PENALTY_AMOUNT,transaction_type
from vw_shipment_penalty
where shipment_invoice_id=$P{p_invoice_id}
and transaction_type = upper($P{p_transaction_type})]]>
		</queryString>
		<field name="ELEMENTS" class="java.lang.String"/>
		<field name="PENALTY_COST_PER_UNIT" class="java.math.BigDecimal"/>
		<field name="DEDUCT_FOR_EACH" class="java.math.BigDecimal"/>
		<field name="DEDUCT_CONDITION_VALUE" class="java.math.BigDecimal"/>
		<field name="QUANTITY_MNT_UNT" class="java.lang.String"/>
		<field name="PENALTY_AMOUNT" class="java.math.BigDecimal"/>
		<field name="TRANSACTION_TYPE" class="java.lang.String"/>
	</subDataset>
