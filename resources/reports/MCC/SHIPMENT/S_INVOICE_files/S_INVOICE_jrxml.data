		<parameter name="p_invoice_id" class="java.math.BigDecimal"/>
		<queryString language="SQL">
			<![CDATA[SELECT SP.*,
PC.PERCENT,LOWER(PC.CONDITION)CONDITION,PC.CONDITION_VALUE_1,PC.CONDITION_VALUE_2,PC.CONDITION_MNT_UNIT,
(case when type='U' then
(SELECT WORDS FROM VW_PAYABLE_CONDITION_WORDS WHERE ID=PC.CONDITION_ID)
