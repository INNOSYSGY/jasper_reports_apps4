	<subDataset name="Over_Under" uuid="e71141aa-d6ac-403f-8c3e-8d3177895dc2">
		<parameter name="P_SHIPMENT_INVOICE_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
d.name element_name,
d.symbol element_symbol,
a.over_under_price over_under_total,
s.over_under_qp_start,
s.over_under_qp_end,
'Over/Under: '||pkg_mcc.get_qp_display(s.over_under_qp_start,s.over_under_qp_end) qp_display,
(
select code
from price_source c
where c.id=b.price_source
) Price_Source
from shipment_payable a
join shipment_invoice e on a.shipment_invoice_id=e.id
join payable_condition b on a.payable_condition_id=b.id
join element d on a.element_id=d.id
join shipment s on e.shipment_id=s.id
where shipment_invoice_id=$P{P_SHIPMENT_INVOICE_ID}
order by d.symbol asc]]>
		</queryString>
		<field name="ELEMENT_NAME" class="java.lang.String"/>
		<field name="ELEMENT_SYMBOL" class="java.lang.String"/>
		<field name="OVER_UNDER_TOTAL" class="java.math.BigDecimal"/>
		<field name="OVER_UNDER_QP_START" class="java.sql.Timestamp"/>
		<field name="OVER_UNDER_QP_END" class="java.sql.Timestamp"/>
		<field name="QP_DISPLAY" class="java.lang.String"/>
		<field name="PRICE_SOURCE" class="java.lang.String"/>
	</subDataset>
	<parameter name="p_invoice_id" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select h.*,
case when upper(invoice_type)='FINAL' THEN
DECODE(upper(h.transaction_type),'PURCHASE','Payment Made','Payment Received')
else
PROVISIONAL_PRECENTAGE||' Provisional Value'
END Provisional_Label,
(case when upper(invoice_type) in ('FINAL','PROVISIONAL') AND NVL(provisional_payment,0)<>0  then
provisional_payment
else
adjusted_provisional_payment
end) report_provisional_value,b.*,
case when (
select sum(abs(over_under_total))
from shipment_payable a
where shipment_invoice_id=h.invoice_id
)>0 then 'Y' else 'N' end has_Over_under,
case when upper(TRANSACTION_TYPE)='PURCHASE' and upper(invoice_type)='FINAL' AND PAYMENT_INSTRUCTION_ID is not null  THEN
'Y'
else 'N'
end show_instruction
from  vw_shipment_invoice_header h
cross join
(
select u.*,b.*
from vw_software_user u
cross join vw_software_user_bank b
) b
where invoice_id=$P{p_invoice_id}]]>
	</queryString>
