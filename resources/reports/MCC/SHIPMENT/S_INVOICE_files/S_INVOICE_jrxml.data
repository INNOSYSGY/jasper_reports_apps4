	<subDataset name="Shipment_Penalty" uuid="62ae6be4-bcb8-4842-9fb0-8afd556f95c5">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Metallica Trading"/>
		<parameter name="p_invoice_id" class="java.math.BigDecimal"/>
		<parameter name="p_transaction_type" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT
distinct
pkg_mcc.f_return_elements (ELEMENTS) Elements,
PENALTY_COST_PER_UNIT,
DEDUCT_FOR_EACH,
DEDUCT_CONDITION_VALUE,
nvl((case
when QUANTITY_MNT_UNT = '%' then QUANTITY_MNT_UNT
else ' '||QUANTITY_MNT_UNT end),'') QUANTITY_MNT_UNT,
PENALTY_AMOUNT,transaction_type,
condition_description
from vw_shipment_penalty
where shipment_invoice_id=$P{p_invoice_id}
and transaction_type = upper($P{p_transaction_type})
and PENALTY_AMOUNT>=0
order by ELEMENTS,DEDUCT_CONDITION_VALUE]]>
