			<![CDATA[select distinct rownum,sp.element,sp.refining_charge,sp.REFINING_CHARGE_PER refining_cost,nvl(f_get_mmt_unit(sp.REFINING_CHARGE_MMT_UNIT),' ') refining_charge_unit
