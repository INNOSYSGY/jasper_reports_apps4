	<subDataset name="Shipment_Deductions" uuid="62ae6be4-bcb8-4842-9fb0-8afd556f95c5">
		<parameter name="p_invoice_id" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="p_transaction_type" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[Select distinct
Initcap(D.Deduction_Type) Deduction_Type,
D.Deduction_Amount,
/*Case When V.Transaction_Type='SALE' Then Buyer_Contract.Treatment_Charge
Else Purchase_Contract.Treatment_Charge End Treatment_Rate,
decode(F_Get_Mmt_Unit((Case When V.Transaction_Type='SALE' Then Buyer_Contract.Treatment_Charge_Mmt_Unit
Else purchase_contract.treatment_charge_mmt_unit End)),null,'  ','per '||F_Get_Mmt_Unit((Case When V.Transaction_Type='SALE' Then Buyer_Contract.Treatment_Charge_Mmt_Unit
Else purchase_contract.treatment_charge_mmt_unit End)))
  Treatment_Charge_Mmt_Unit*/
deduct Treatment_Rate,F_Get_Mmt_Unit(d.DEDUCT_MMT_UNIT) Treatment_Charge_Mmt_Unit
From Shipment_Deduction D
join shipment_invoice v on d.shipment_invoice_id=v.id
Join Shipment S On D.Shipment_Id=S.Id
Join Agreement A On A.Id=S.Agreement_Id
Join Contract Buyer_Contract  On A.Sale_Contract_Id=Buyer_Contract.Id
Join Contract Purchase_Contract  ON a.purchase_contract_id=Purchase_Contract.id
where shipment_invoice_id=$P{p_invoice_id}
and deduction_amount>0]]>
		</queryString>
		<field name="DEDUCTION_TYPE" class="java.lang.String"/>
		<field name="DEDUCTION_AMOUNT" class="java.math.BigDecimal"/>
		<field name="TREATMENT_RATE" class="java.math.BigDecimal"/>
		<field name="TREATMENT_CHARGE_MMT_UNIT" class="java.lang.String"/>
	</subDataset>
