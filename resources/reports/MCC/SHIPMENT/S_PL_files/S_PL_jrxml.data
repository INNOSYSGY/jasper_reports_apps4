	<subDataset name="Expense" uuid="8a8fe20d-3ac6-411f-a80d-d9aa5cfade34">
		<parameter name="P_SHIPMENT_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
shipment_id,
A.Margin_Sheet_Id,
A.Expense_Type,
A.Expense_Type_Code,
A.Estimate_Amounted,
A.Shipment_Status,
measurement_type,
case when measurement_type='TON' then
a.actual_amount
else null end actual_unit_amount,
case when measurement_type='TON' then
a.actual_amount*tonnage
else a.actual_amount end actual_amount
from vw_shipment_expense_projection a
where shipment_id=$P{P_SHIPMENT_ID}]]>
		</queryString>
		<field name="SHIPMENT_ID" class="java.math.BigDecimal"/>
		<field name="MARGIN_SHEET_ID" class="java.lang.String"/>
		<field name="EXPENSE_TYPE" class="java.lang.String"/>
		<field name="EXPENSE_TYPE_CODE" class="java.math.BigDecimal"/>
		<field name="ESTIMATE_AMOUNTED" class="java.math.BigDecimal"/>
		<field name="SHIPMENT_STATUS" class="java.lang.String"/>
		<field name="MEASUREMENT_TYPE" class="java.lang.String"/>
		<field name="ACTUAL_UNIT_AMOUNT" class="java.math.BigDecimal"/>
		<field name="ACTUAL_AMOUNT" class="java.math.BigDecimal"/>
		<sortField name="EXPENSE_TYPE"/>
	</subDataset>
	<subDataset name="shipment fund" uuid="80a3c3ee-cc63-412d-9850-eec019cffbfc">
		<parameter name="P_SHIPMENT_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select f.shipment_id,

				--------------Financing-------------
        f.amount_financed,
				f.interest_rate interest,
				f.period days,
				pkg_shipment_pl.get_shipment_finance_payment(f.shipment_id) finance_payment,
                                round(f.amount_financed*(f.interest_rate/100)*(f.period/365),2) financing_est,
                                round(f.amount_financed*(f.interest_rate/100)*(f.period/365),2) financing_actual,
f.percent_financed,
f.invoice_amount
from
shipment_funding f
where shipment_id=$P{P_SHIPMENT_ID}]]>
		</queryString>
		<field name="SHIPMENT_ID" class="java.math.BigDecimal"/>
		<field name="AMOUNT_FINANCED" class="java.math.BigDecimal"/>
		<field name="INTEREST" class="java.math.BigDecimal"/>
		<field name="DAYS" class="java.math.BigDecimal"/>
		<field name="FINANCE_PAYMENT" class="java.math.BigDecimal"/>
		<field name="FINANCING_EST" class="java.math.BigDecimal"/>
		<field name="FINANCING_ACTUAL" class="java.math.BigDecimal"/>
		<field name="PERCENT_FINANCED" class="java.math.BigDecimal"/>
		<field name="INVOICE_AMOUNT" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="p_shipment_id" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[select s.id shipment_id,
                 (select name
                    from vw_contact_details c
                    where c.id=s.supplier) purchase_from,
             (select name
                from vw_contact_details c
                where c.id=s.buyer) sold_to,
                (
                select reference_number
                from agreement a
                where a.id=s.agreement_id
                )||'-'||
            s.shipment_number contract_no,
             pkg_shipment_pl.get_shipment_weight(s.id) tonnage,
                pkg_shipment_pl.get_shipment_invoice_total(s.id,'SALE') sale_concept_gain,
pkg_shipment_pl.get_shipment_status(s.id) status,             pkg_shipment_pl.get_shipment_invoice_total(s.id,'PURCHASE') purchase_concept_gain,
                pkg_shipment_pl.get_shipment_gain_total(s.id) Contractual_Gain,
                pkg_shipment_pl.get_shipment_total_expense(s.id)shipment_total_expense,
                pkg_shipment_pl.get_shipment_net_gain(s.id) net_gain,
                pkg_shipment_pl.get_profit_per_ton(s.id) profit_per_gain,
                pkg_shipment_pl.get_shipment_hedge_loss(s.id) hedge_loss,
                pkg_shipment_pl.get_shipment_total_admin_cost(s.id) total_admin_cost,
                pkg_shipment_pl.get_shipment_value(s.id,'FINAL','PURCHASE') Purchase_final,
				null purchase_provisional_percent,
              pkg_shipment_pl.f_get_shipment_payment(s.id,'PURCHASE','PROVISIONAL') Purchase_provisional,
				--pkg_shipment_pl.get_purchase_payment_due(s.id)
pkg_shipment_pl.get_shipment_value(s.id,'FINAL','PURCHASE') -
pkg_shipment_pl.f_get_shipment_payment(s.id,'PURCHASE','PROVISIONAL') purchase_payment_due,
             pkg_shipment_pl.get_shipment_value(s.id,'FINAL','SALE') Sale_final,
				null sale_provisional_percent,
              pkg_shipment_pl.f_get_shipment_payment(s.id,'SALE','PROVISIONAL') Sale_provisional,
				--pkg_shipment_pl.get_sale_payment_due(s.id) Sale_Payment_due,
pkg_shipment_pl.get_shipment_value(s.id,'FINAL','SALE')
-
pkg_shipment_pl.f_get_shipment_payment(s.id,'SALE','PROVISIONAL')
as Sale_Payment_Due,
pkg_shipment_pl.get_net_statement_amount(s.id)Net_settlement,
				--------------Financing-------------
				f.interest_rate interest,
				f.period days,
				pkg_shipment_pl.get_shipment_finance_payment(s.id) finance_payment,
                                pkg_shipment_pl.get_shipment_financing(s.id,'ESTIMATE') financing_est,
                                pkg_shipment_pl.get_shipment_financing(s.id,'ACTUAL') financing_actual,
pkg_shipment_pl.GET_SHIPMENT_MARGIN_NET_PROFIT(s.id) margin_net_profit,                                f.amount_financed,f.percent_financed,f.invoice_amount,
b.*

from shipment s
left outer join shipment_funding f on s.id=f.shipment_id
cross join
(
select u.*
from vw_software_user u

) b
where s.id=$P{p_shipment_id}]]>
	</queryString>
