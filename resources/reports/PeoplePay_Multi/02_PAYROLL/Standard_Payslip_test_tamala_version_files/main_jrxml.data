		<queryString>
			<![CDATA[select 'PAYE' description, tax_amount amount_deducted, tax_amount_ytd amount_deducted_ytd from pa_pmg_payrolldtl
where id=$P{P_PAY_SLIP_ID}
and tax_amount > 0
union
select 'NIS' description, nis_employee amount_deducted, nis_employee_ytd amount_deducted_ytd from pa_pmg_payrolldtl
where id=$P{P_PAY_SLIP_ID}
and nis_employee > 0
union
select to_char(ded.description) description, pd.amount_deducted amount_deducted, pd.amount_deducted_ytd
from PA_PMG_PAYROLLDEDUCTION pd join PA_PCF_ADMINDEDUCTION adm ON adm.id = pd.adm_deduction_id
join PA_PCF_DEDUCTIONCODE ded ON adm.expense_id = ded.id
where
(upper(ded.Who_Pays) ='EMPLOYEE' or upper(adm.Deduction_Type)='EMPLOYEE')
and amount_deducted>0
and ded.org_id=pkg_global_fnts.fn_shareRefOrg($P{P_ORG_ID})  
        and pd.pay_slipno=$P{P_PAY_SLIP_ID}]]>
		</queryString>
