	<parameter name="PAYSLIP_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[select
pa.org_id,
pa.pay_batch_id,
to_char(date_employed,'yyyy-mm-dd') doe,
pa.id pay_slip_no,pa.surname_hist,pa.first_name_hist,pa.salary_id,pa.period_start,pa.period_end,
pa.payroll_effective,pa.department_name_hist,upper(pa.organisation_name) organisation_name,initcap(PA.PAYMENT_TYPE_HIST)PAYMENT_TYPE_HIST,
REPLACE(TO_CHAR(pa.basic_personal_rate,'999,999,999'),' ','') basic_personal_rate,
pa.basic_personal_rate_ytd,
pa.org_id,ep.emp_company_no emp_id,pa.id pay_slip,
NVL(PA.SURNAME_HIST,'')||', '||NVL(PA.FIRST_NAME_HIST,'')||' '||NVL(R.MIDDLE_NAME,'') EMPLOYEE_NAME,
initcap(nvl(v.unit_name,'Unknown')) WORK_LOCATION,
initcap(pa.POSITION_NAME_HIST) position_name_hist ,pa.surname_hist,pa.first_name_hist,
case when pa.org_id= 29705 then pa.total_deduction-pa.tax_amount else pa.total_deduction end total_deduction
,pa.gross_taxable+pa.gross_nontaxable  AS gross,
pa.time_tax_earnings,pa.nis_employee,
case when pa.org_id= 29705 then  0 else pa.tax_amount end tax_amount,
case when pa.org_id= 29705 then 
        pa.net_pay + pa.tax_amount 
        else pa.net_pay end net_pay,
pa.gross_taxable_ytd+pa.gross_nontaxable_ytd gross_ytd,
pa.nis_employee_ytd,
time_tax_earnings_ytd,
case when pa.org_id= 29705 then pa.net_pay_ytd + pa.tax_amount_ytd else pa.net_pay_ytd end net_pay_ytd,
case when pa.org_id= 29705 then pa.total_deduction_ytd - pa.tax_amount_ytd else pa.total_deduction_ytd end total_deduction_ytd,
case when pa.org_id= 29705 then 0 else pa.tax_amount_ytd end tax_amount_ytd,
(case when manager_approved_by is null then 'N' else 'Y' end) approved,
Pkg_Global_Fnts.Fn_Getcurnistaxno(r.Id , 'TIN',  pa.PERIOD_START ) Tax_no,
nvl('- '||P.POSITION_NAME, '') position_name,
eap.START_PAID_FOR, 
eap.END_PAID_FOR,
ho.photo org_photo
from pa_pmg_payrolldtl pa
join PA_PMG_PAYROLLHD ph on pa.pay_batch_id=ph.id
join PA_PCF_EARNINGPERIOD eap on eap.id=pa.earnings_period_id
join HR_RCM_EMPLOYEE ep on pa.emp_id=ep.id
join HR_RCM_INDIVIDUAL r on ep.ind_id=r.id
join hr_hcf_organisation ho ON PA.ORG_ID=HO.ID
left join HR_HCF_POSITION P on P.id=ep.position_id
left outer join vw_organisation_structure v on pa.orgdtl_id=v.id
WHERE PAY_BATCH_ID=$P{ID}
and pa.id=case when  $P{PAYSLIP_ID}  is null then pa.id else  $P{PAYSLIP_ID}  end
order by nvl(v.unit_name,'Unknown'),pa.surname_hist,pa.first_name_hist]]>
	</queryString>
