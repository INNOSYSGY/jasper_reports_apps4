		<queryString>
			<![CDATA[select pd.id pay_slipno, pd.basic_personal_rate income_amount, pd.basic_personal_rate_ytd income_amount_ytd, 'Basic Rate' description, 'NR' income_code, pd.emp_id
 from pa_pmg_payrolldtl pd
where id = $P{P_PAY_SLIP_ID}
union all
SELECT O.PAY_SLIPNO,  ROUND(O.INCOME_AMOUNT,2) INCOME_AMOUNT, INCOME_AMOUNT_YTD,
    to_char(CASE WHEN O.INCOME_CODE IN ('OT1','OT', 'NR') THEN
        ic.Description||' ('||quantity||'hr(s)' ||'/'||REPLACE(TO_CHAR(o.rate,'FML999G999G999G999G990D00'),' ','')||')'
        ELSE ic.Description END)Description, to_char(o.Income_code) income_code, O.Emp_id
    FROM   PA_PMG_PAYROLLINCOME O INNER JOIN PA_PCF_INCOMECODE IC ON O.INCOME_CODE = IC.INCOME_CODE
    where	pay_slipno=  $P{P_PAY_SLIP_ID}
