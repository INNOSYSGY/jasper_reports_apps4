	<queryString>
		<![CDATA[select distinct py.id, py.emp_company_no_hist , py.surname_hist || ', ' || py.first_name_hist || ' ' || py.middle_name EMPNO_NAME,
        od.coa_code, od.unit_name,         
case when py.PAYMENT_TYPE_HIST='MONTHLY' then py.BASIC_PERSONAL_RATE 
else 
nvl((select sum(c.INCOME_AMOUNT) from PA_PMG_PAYROLLINCOME c where c.PAY_SLIPNO=py.id and  c.INCOME_CODE='NR'),0)
end "Basic Pay",        
        nvl("Non Taxable Allow",0) "Non Taxable Allow",
		 nvl("Taxable Allow.",0) "Taxable Allow.",
        (py.gross_taxable+py.gross_nontaxable) "Gross Pay",
         tax_amount "Paye Deduct", nis_employee "NIS Employee Deduct",
        medical_sum "Medical Employee",  py.total_deduction, py.net_pay,
        nvl("Overtime Pay",0) "Overtime Pay",
        nvl((select nvl(amount_deducted,0) 
        from pa_pmg_payrolldeduction
        where  expense_code = 'PENSION' and deduction_type = 'EMPLOYEE'
        and pay_slipno=py.id),0) "Pension Employee",
        nvl(O_DEDUCT,0) "Other Deduction",
        (py.gross_taxable-py.nis_employee)-
        --sum(py.gross_taxable)-sum(py.nis_employee)-
        (select  nvl(sum(a.amount_deducted),0)
		    from pa_pmg_payrolldeduction a join pa_pcf_admindeduction b on a.adm_deduction_id=b.id
		    join pa_pcf_deductioncode c on c.id=b.expense_id
		    Where C.Reduce_Tax_Nis_Total=1 and a.PAY_SLIPNO=py.id)  "Total Taxable",
        py.org_id,
        (SELECT TO_CHAR( ph.PAYROLL_DATE, 'FMMonth DD, YYYY' ) FROM dual) payroll_date,
        initcap(PKG_GLOBAL_FNTS.Get_Lookup_Col(ep.employment_class_id, 'VALUE_DESCRIPTION' ) ||' '||py.PAYMENT_TYPE_HIST) EMP_CLASS,
        TO_CHAR(ep.start_date,'FMMonth DD, YYYY' ) ||' -- '|| TO_CHAR(ep.end_date, 'FMMonth DD, YYYY' ) ||' '||REPLACE(initcap(py.compute_gross_hist),'_',' ') PDATE
from pa_pmg_payrolldtl py
full join (select nvl(sum(case when p.income_code = 'OT' then nvl(p.INCOME_AMOUNT,0) else 0  end +
            case when p.income_code = 'OT1' then nvl(p.INCOME_AMOUNT,0) else 0 end),0) "Overtime Pay",
            nvl(sum(case when p.income_code <> 'OT1' and p.income_code <> 'OT' and p.taxable = 1 and pdtl.payment_type_hist <> 'FORTNIGHTLY' then nvl(p.INCOME_AMOUNT,0)
            when p.income_code <> 'OT1' and p.income_code <> 'OT' and p.income_code <> 'NR' and p.taxable = 1 and pdtl.payment_type_hist = 'FORTNIGHTLY' then nvl(p.INCOME_AMOUNT,0) else 0 end),0) "Taxable Allow.",
            nvl(sum(case when p.income_code <> 'OT1' and p.income_code <> 'OT' and p.taxable = 0 then nvl(INCOME_AMOUNT,0) else 0 end),0) "Non Taxable Allow",
            pay_slipno
            from pa_pmg_payrollincome p
            join hr_rcm_employee emp on emp.id = p.emp_id
            join pa_pmg_payrolldtl pdtl on pdtl.id = p.pay_slipno
            group by pay_slipno
            ) ic on ic.pay_slipno = py.id
join hr_hcf_orgstructuredtl od on od.id = py.orgdtl_id
full join (select pay_slipno,
       sum(nvl(amount_deducted,0)) O_DEDUCT
        from pa_pmg_payrolldeduction
        where  expense_code <> 'MED' and expense_code <> 'PENSION' and deduction_type = 'EMPLOYEE' 
        group by pay_slipno 
        ) pd on pd.pay_slipno = py.id
join hr_rcm_employee e on e.id = py.emp_id
left join ( select personal_rate, emp_id from hr_rcm_salary where (end_date is null or end_date >= sysdate)) sal on sal.emp_id = py.emp_id
join hr_hcf_position w on w.id = e.position_id 
join pa_pmg_payrollhd ph on ph.earnings_period_id = py.earnings_period_id
join pa_pcf_earningperiod ep on ep.id = ph.earnings_period_id
where py.earnings_period_id =  $P{EARNINGS_PERIOD_ID} 
and py.org_id =  $P{IN_ORG_ID} 
and 
( $P{allEmployees} = 1 or 
exists (select 1
                        from RPT_PARAMETER w1
                        WHERE 
                        W1.SESSION_ID=  $P{SESSION_ID} 
                         and
                         option_value=case when option_name = $P{SEARCH_OPTION}  and option_name ='EMPLOYEE' then to_char(py.emp_id) 
                                                when option_name = $P{SEARCH_OPTION}  and option_name ='GRADE_SCALE' then to_char(w.grade_id)
                                                when option_name = $P{SEARCH_OPTION}  and option_name ='ORG_STRUCT_LEVEL' then to_char(e.orgdtl_id)
                                                when option_name = $P{SEARCH_OPTION}  and option_name ='POSITION' then to_char(e.position_id)
                                                when option_name = $P{SEARCH_OPTION}  and option_name ='WORK_LOCATION' then to_char(e.wklocation_id)
                                                when option_name = $P{SEARCH_OPTION}  and option_name ='UNION' then to_char(e.union_code_id)
                                                when option_name = $P{SEARCH_OPTION}  and option_name ='EMPLOYMENT_TYPE' then to_char(e.EMPLOYMENT_TYPE_ID)
                                                end
                        )
) 
group by py.id, py.surname_hist, py.first_name_hist, py.middle_name, od.coa_code, od.unit_name, py.basic_personal_rate, 
 "Taxable Allow.", "Non Taxable Allow", py.gross_taxable,py.gross_nontaxable, tax_amount, nis_employee, 
medical_sum, py.total_deduction, py.net_pay, "Overtime Pay", freepay, py.org_id,ph.PAYROLL_DATE,ep.employment_class_id,
ep.start_date,ep.end_date, py.compute_gross_hist,  O_DEDUCT, py.PAYMENT_TYPE_HIST,  py.emp_company_no_hist]]>
	</queryString>
