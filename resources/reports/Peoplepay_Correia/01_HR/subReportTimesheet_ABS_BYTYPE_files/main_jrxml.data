	<queryString language="SQL">
		<![CDATA[select
EMP.ID emp_id,
dept.id department_id,
emp.emp_company_no,
nvl(p.position_name,'') designation,
Initcap(trim(ind.Surname)) || ', ' || Initcap(trim(ind.First_Name)) || ' ' || nvl(Initcap(trim(ind.Middle_Name)),'') Name,
Initcap(trim(ind.Surname)) || ', ' || Initcap(trim(ind.First_Name)) || ' ' || nvl(Initcap(trim(ind.Middle_Name)),'') || ' ('||emp.emp_company_no ||')' FULL_Name,
dept.unit_name department,
ch.work_date,
'D'||lpad(EXTRACT(DAY FROM ch.work_date),2,'0') day_numberc, 
to_char(ch.work_date,'DD-MON') short_work_date,
to_char( ch.work_date, 'DY') day_letter,
to_char(ch.min_time_in, 'HH:MIPM') min_time_in,
to_char(ch.max_time_out, 'HH:MIPM') max_time_out,
NVL(H.HOLIDAY_CODE,'-') holiday,
shft.shift_code,
nvl(ch.hours_worked,0) hours_worked,
nvl(ch.assign_shift_hours,0) assign_shift_hours,
nvl(ch.normal_time,0) calculated_shift_hours_worked,

--calculated absences vs keyed absences vs approved absences vs deducted absences
nvl(ch.total_time_lost,0) calculated_abs,
nvl(ch.approved_abs,0) approved_abs,
nvl(( select sum(absence_hours) 
                            from hr_att_timesheetabsence ta
                            where ta.emp_id = ch.emp_id and ta.work_date = ch.work_date
                            and upper(pkg_global_fnts.status_descript(status)) != 'DISAPPROVED'
 ),0) keyed_abs,
 
 nvl(( select sum(absence_hours) 
                            from hr_att_timesheetabsence ta
                            where ta.emp_id = ch.emp_id and ta.work_date = ch.work_date
                            and ta.affect_salary = 'DEDUCTION'
                            and upper(pkg_global_fnts.status_descript(status)) != 'DISAPPROVED'
 ),0) keyed_abs_deduction,
 
--calculated overtime vs keyed overtime vs approved overtime
nvl(ch.over_time,0) calculated_ot,
nvl(ch.approved_overtime,0) approved_ot,
nvl(( select sum(case when (income_code in ( 'OT','OT1','HOL1.5', 'HOL2') ) then quantity else 0 end) 
                            from hr_att_timesheetincome ti
                            where ti.emp_id =ch.emp_id and ti.work_date = ch.work_date
                            and upper(pkg_global_fnts.status_descript(status)) != 'DISAPPROVED'
),0) keyed_ot,

 --keyed overall income vs approved income
 nvl(( select sum(quantity) 
                            from hr_att_timesheetincome ti
                            where ti.emp_id =ch.emp_id and ti.work_date = ch.work_date
                            and upper(pkg_global_fnts.status_descript(status))  IN ('APPROVED','VERIFY', 'VERIFIED')                           
),0) approved_income,
nvl(( select sum(quantity) 
                            from hr_att_timesheetincome ti
                            where ti.emp_id =ch.emp_id and ti.work_date = ch.work_date
                            and upper(pkg_global_fnts.status_descript(status)) != 'DISAPPROVED'
),0) keyed_income,

--fn_chk_incomedesc(tc.empent_id)||' ('||REASON_INCOME||')' description,
nvl(tinctabs.ttype,'') ttype,
nvl(tinctabs.ttypedesc,'') ttypedesc,
nvl(tinctabs.quantity,0) quantity,
nvl(tinctabs.start_time,'') time_in,
nvl(tinctabs.end_time,'') time_out,
nvl(tinctabs.adesc, '') adesc,
nvl(tinctabs.acode, '') acode,
nvl(tinctabs.approved_by,'') approved_by,
nvl(tinctabs.approved_date,'') approved_date,
nvl(tinctabs.status,'') status,
nvl(tinctabs.affect_salary,'') affect_salary,

nvl(ch.approved_by,'') ch_approved_by,
upper(pkg_global_fnts.status_descript(ch.status)) ch_status

from hr_att_empcalendar_hd ch
--tinctabs - time incomes/time absences union to show all child records in one area
 join (
                        select 'ABS' as ttype, 'ABSENCES' as ttypedesc,emp_id,work_date, null as start_time,null as end_time,absence_hours quantity,
                        INITCAP(pkg_global_fnts.get_lookup_col(ta.leave_type, 'VALUE_DESCRIPTION')) adesc,
                        pkg_global_fnts.get_lookup_col(ta.leave_type, 'TABLE_VALUE') acode,
                        approved_date,CAST(approved_by AS NVARCHAR2(300)) approved_by,
                        upper(pkg_global_fnts.status_descript(status)) STATUS,
                        --case when affect_salary in ('DEDUCTION','PAYMENT') then affect_salary else '' end affect_salary
                        affect_salary
                        from HR_ATT_TIMESHEETABSENCE ta --where ch.work_date = ta.work_date and ch.emp_id = ta.emp_id                           

                    ) tinctabs on ch.emp_id = tinctabs.emp_id and ch.work_date = tinctabs.work_date
   join hr_rcm_employee emp on emp.id = ch.emp_id
   join hr_rcm_individual ind on emp.ind_id = ind.id
    left outer join hr_att_shift shft on ch.assigned_shift = shft.id and shft.org_id =  $P{IN_ORG_ID}
    left outer join HR_HCF_HOLIDAY H ON ch.WORK_DATE = H.HOLIDAY_DATE and h.org_id = $P{IN_ORG_ID}
    left outer join Hr_Hcf_Orgstructuredtl dept on ch.orgdtl_id = dept.id
    left outer join Hr_Hcf_position p on ch.position_id = p.id  
where ch.orgdtl_id =  nvl( $P{DEPARTMENT_ID} , ch.orgdtl_id) 
	and ch.emp_id =  nvl( $P{EMP_ID}, ch.emp_id)
	and ch.work_date between  trunc(to_date($P{START_DATE})) and trunc(to_date($P{END_DATE}))
	and ch.org_id = $P{IN_ORG_ID}

 and exists (select 1
				          from HR_HCF_ORGSTRUCTUREDTL T left outer join VW_USERORGSTRUCT k on ( t.id = k.ORG_STRUCTURE_ID  and nvl(k.user_name,'1') = decode(k.user_name, null, '1', null))
				          where  t.id = emp.ORGDTL_ID
				          and emp.ORGDTL_ID=decode(pkg_security.fn_testfinegradeOrgStruct((select user_name from tbluser where employee_no = emp.emp_company_no)), 0, emp.ORGDTL_ID, k.org_structure_id))]]>
	</queryString>
