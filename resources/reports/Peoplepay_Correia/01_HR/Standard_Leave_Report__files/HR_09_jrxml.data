		<queryString language="SQL">
			<![CDATA[select 
LTY.LEAVE_CODE, lty.leave_description,  
LT.LV_TRACK_EFFECTIVE_DATE, Lt.end_date,
TRUNC(lt.brought_fwd,2) BROUGHT_FWD,TRUNC(lt.carry_fwd,2) CARRY_FWD,
TRUNC(LT.LEAVE_EARN,2) LEAVE_EARN,TRUNC( lt.ACCRUED,2) ACCRUED, TRUNC( lt.PENDING,2) PENDING, TRUNC(lt.USED,2) USED,
TRUNC(lt.CR_ADJUSTMENTS,2) CR_ADJUSTMENTS, TRUNC(lt.DR_ADJUSTMENTS,2) DR_ADJUSTMENTS,
(select nvl(sum(nvl(actual_days,0)),0) from HR_LV_LEAVEHISTORY LHK where LHK.emp_id=LT.emp_id and LHK.leave_type_ID=LT.leave_type_ID and LHK.FROMDATE between LT.LV_TRACK_EFFECTIVE_DATE and Lt.end_date and substr(upper(pkg_global_fnts.status_descript(LHK.status)),1,3) = 'KEY') Keyed,
(select nvl(sum(nvl(actual_days,0)),0) from HR_LV_LEAVEHISTORY LHK where LHK.emp_id=LT.emp_id and LHK.leave_type_ID=LT.leave_type_ID and LHK.FROMDATE between LT.LV_TRACK_EFFECTIVE_DATE and Lt.end_date and substr(upper(pkg_global_fnts.status_descript(LHK.status)),1,3) = 'APP') Approved,
(select nvl(sum(nvl(actual_days,0)),0) from HR_LV_LEAVEHISTORY LHK where LHK.emp_id=LT.emp_id and LHK.leave_type_ID=LT.leave_type_ID and LHK.FROMDATE between LT.LV_TRACK_EFFECTIVE_DATE and Lt.end_date and substr(upper(pkg_global_fnts.status_descript(LHK.status)),1,3) = 'VER') Verified,
(select nvl(sum(nvl(actual_days,0)),0) from HR_LV_LEAVEHISTORY LHK where LHK.emp_id=LT.emp_id and LHK.leave_type_ID=LT.leave_type_ID and LHK.FROMDATE between LT.LV_TRACK_EFFECTIVE_DATE and Lt.end_date and substr(upper(pkg_global_fnts.status_descript(LHK.status)),1,3) = 'CAN') Cancelled,
(select nvl(sum(nvl(actual_days,0)),0) from HR_LV_LEAVEHISTORY LHK where LHK.emp_id=LT.emp_id and LHK.leave_type_ID=LT.leave_type_ID and LHK.FROMDATE between LT.LV_TRACK_EFFECTIVE_DATE and Lt.end_date and substr(upper(pkg_global_fnts.status_descript(LHK.status)),1,3) not in ('KEY','APP','VER','CAN')) Other
from HR_LV_LEAVETRACK LT
join HR_LV_LEAVETYPE lty on lty.id=LT.LEAVE_TYPE_ID
where LT.emp_id= $P{emp_id} 
and LT.LEAVE_TYPE_ID=  case when $P{Leave_type_id} = 0 then LT.LEAVE_TYPE_ID else  $P{Leave_type_id}    end
and (
        (lty.leave_code = 'AL'
          and
          (
            (trunc(sysdate) between trunc(lt.LV_TRACK_EFFECTIVE_DATE) and trunc(lt.END_DATE)) 
             OR (add_months( sysdate, -12) between trunc(lt.LV_TRACK_EFFECTIVE_DATE) and trunc(lt.END_DATE))
          )
         )
         OR
         trunc(sysdate) between trunc(lt.LV_TRACK_EFFECTIVE_DATE) and trunc(lt.END_DATE)
    )
and LTY.PLANNED_OR_UNPLANNED='PLANNED']]>
