		<![CDATA[select
e.id emp_id,
e.emp_company_no||'-'||i.first_name||' '||i.surname||case when date_separated is not null then ' (Sep. '||to_char(date_separated,'DD-MON-YYYY')||')' end employee_name, 
e.date_employed, e.employment_class,o.unit_name department, p.position_name
from HR_RCM_EMPLOYEE e 
join hr_rcm_individual i on e.ind_id=i.id
join HR_HCF_ORGSTRUCTUREDTL o on o.id=e.orgdtl_id
join hr_hcf_position p on p.id=e.position_id
where (e.date_separated is null or e.date_separated >= to_date( $P{from_date},'YYYY-MM-DD'))
AND E.EMPLOYMENT_CLASS not like 'HRMIS%'
and e.id = case when $P{emp_id} = 0 then e.id else $P{emp_id}  end
and o.id = case when   $P{Department_id} = 0 then o.id else  $P{Department_id}   end
