		<queryString>
			<![CDATA[select      
        LTY.LEAVE_CODE LEAVE_TYPE,
         LTY.LEAVE_description,
        case when a.TRANSACTION_TYPE = 'CREDIT' then 'CR' 
                when a.TRANSACTION_TYPE = 'DEBIT' then 'DR' 
                else 'X' 
        end CR_DR, 
        a.TRANSACTION_VALUE,
       count(a.id) cnt,
       ROUND(sum(a.AMOUNT),2) amount
  from HR_LV_LEAVETRANSACTION a   
  join HR_LV_LEAVETYPE LTY on LTY .id=a.LEAVE_ID
where emp_id = $P{emp_id} 
and a.transaction_date between to_date(   $P{from_date}  ,'yyyy-mm-dd' ) and TO_DATE(  $P{todate}   ,'yyyy-mm-dd' )
