		<queryString>
			<![CDATA[select  LTY.LEAVE_CODE LEAVE_TYPE, LTY.LEAVE_DESCRIPTION ||'['||
		(select to_char(sum(LH.ACTUAL_DAYS)) 
		 from HR_LV_LEAVEHISTORY LH 
		 where lh.emp_id = lhk.emp_id 
		 and lh.leave_request_type_id = lhk.leave_request_type_id 
		 and lh.fromdate between $P{From_date}  and  $P{To_date} 
		) || ']' LEAVE_DESCRIPTION,
LHK.FROMDATE,LHK.TODATE, LHK.ACTUAL_DAYS DAYS_TAKEN,
  pkg_global_fnts.status_descript(lhk.STATUS) status
from HR_LV_LEAVEHISTORY LHK 
join HR_LV_LEAVETYPE LTY on LTY.id=LHK.LEAVE_REQUEST_TYPE_ID
where LHK.FROMDATE between  $P{From_date} and $P{To_date} 
and LHK.EMP_ID=  $P{Emp_id}
and LHK.LEAVE_TYPE_ID= $P{Leave_type_id}
and LTY.PLANNED_OR_UNPLANNED='PLANNED']]>
		</queryString>
