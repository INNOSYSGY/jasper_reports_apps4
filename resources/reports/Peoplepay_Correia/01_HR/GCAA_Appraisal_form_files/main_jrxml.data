	<queryString>
		<![CDATA[select 
ORG.PHOTO ORG_PHOTO,
ORG.ORGANISATION_NAME,
emp.emp_company_no, initcap(ind.surname || ', ' 
                                       || NVL(ind.first_name, '' ) 
                                       || ' ' 
                                       || NVL(ind.middle_name, '' ) ) as "Full Name",
                                       
(round(((d.APPRAISAL_END_PERIOD-d.APPRAISAL_START_PERIOD)/365.25),3))as year,

floor(((d.APPRAISAL_END_PERIOD-d.APPRAISAL_START_PERIOD)/365.25)) as month ,round(((round(((d.APPRAISAL_END_PERIOD-d.APPRAISAL_START_PERIOD)/365.25),3))-floor(((d.APPRAISAL_END_PERIOD-d.APPRAISAL_START_PERIOD)/365.25)))*12,0)as appraisal_months, 
        to_char(appraisal_start_period,'Month')||'-'||to_char(appraisal_end_period,'Month YYYY') evaluation_period, 
        
(round(((sysdate-emc.START_DATE)/365.25),-1))as years_in_position,  
round(((round(((sysdate-emc.START_DATE)/365.25),3))-floor(((sysdate-emc.START_DATE)/365.25)))*12,0)as months_in_position,
            
            ind.dob as "Date of Birth",
            emp.position_name as "Position Name",
            emp.org_structure as "Designation",
            emp.date_employed as "Date Employed",
            posloc.location_description as "Location",
            grade.grade_code as "Grade Code"
                from HR_RCM_INDIVIDUAL IND
                JOIN HR_RCM_EMPLOYEE EMP on ind.id = emp.ind_id
                JOIN HR_HCF_ORGANISATION ORG ON EMP.ORG_ID=ORG.ID
                JOIN 
                    ( ( SELECT 
                    id, location_description 
                    FROM HR_HCF_WORKLOCATION ) posloc
                    join  hr_hcf_worklocationposition loc 
                    ON posloc.id = loc.wklocation_id ) 
                    ON emp.position_location_id = loc.id      
                JOIN
                (((select id, grade_code
                    from hr_hcf_positiongrade) grade
                    join  hr_hcf_position a
                    on grade.id = a.grade_id) 
                    join HR_HCF_WORKLOCATIONPOSITION C
                    on c.position_id = a.id)
                    on c.ID = emp.position_location_id
                    join HR_APP_MERITINCREMENT d
                    on d.emp_id=emp.id
                    join HR_RCM_EMPCONTRACT emc
                    on emc.emp_id=emp.id]]>
	</queryString>
