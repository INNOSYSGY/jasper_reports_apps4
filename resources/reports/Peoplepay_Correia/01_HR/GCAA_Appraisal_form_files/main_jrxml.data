		<queryString language="SQL">
			<![CDATA[select a.LEAVE_TYPE,b.QUANTITY,count(a.EMPLVENT_ID) as occasions
from hr_lv_leavehistory a
join HR_RCM_EMPLEAVEENTITLE b
on b.id = a.EMPLVENT_ID
join hr_rcm_employee emp
on emp.id = a.emp_id
where extract(year from a.approved_date) =2020
group by emp.id, a.LEAVE_TYPE, b.QUANTITY]]>
