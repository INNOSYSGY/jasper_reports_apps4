	<queryString>
		<![CDATA[select
     LT.ID LEAVETYPE_PKID,
     lp.id policy_pkid,
     lp.lv_accrual_start,
     lp.auto_leave_create_mv,
     lp.created,
     lp.created_by,
     lp.updated,
     lp.updated_by,
     lp.lv_policy_name,
     lp.lv_policy_desc,
     lp.lv_policy_code,
     lp.org_id,
     lp.quantity,
     lp.use_monday,
     lp.use_tuesday,
     lp.use_wednesday,
     lp.use_thursday,
     lp.use_friday,
     lp.use_saturday,
     lp.use_sunday,
    lp.accessible_after,
     lp.use_roster_or_calendar,
     lp.carry_forward,
     lp.auto_verify,
     lp.income_code_id,
     lp.deduction_code_id,
     lp.leave_earned_control,
     lp.use_holidays,
     lp.unit,
     CASE 
     WHEN lp.quota_control = 0 THEN 'None' 
     WHEN lp.quota_control = 1 THEN 'Soft' 
     WHEN lp.quota_control = 2 THEN 'Hard' 
     ELSE ''
     END quota_control,
     lp.make_editable,
     lp.allow_leave_overdraft,
     lp.max_accummulate,
     lp.max_carryforward,
     lp.leave_overdraft,
     lp.cf_rule_option,
     lp.custom_ev_date,
     lp.max_carry_forward,
     lp.yr_end_evaluation_date,
    lp.days_requested_limit,
    
	o.ADDRESS ORG_ADDRESS, o.FAXNUM, o.TELNUM, o.NIS_REG_NO, o.TAX_REG_NO,
	o.photo org_photo,
	o.organisation_name,
    
	ltp.start_date,ltp.end_date,ltp.priority_level,
	lt.leave_code,lt.leave_description
from hr_lv_leavepolicy lp
JOIN HR_HCF_ORGANISATION O ON O.ID = LP.ORG_ID
join hr_lv_leavetype_policy ltp on ltp.policy_id = lp.id
join hr_lv_leavetype lt on ltp.leavetype_id = lt.id
where lp.org_id= $P{ORG_ID}]]>
	</queryString>
