group by emp.id, emp.orgdtl_id, emp.employment_class_id, emp.position_location_id, wk.id, inc.description
union
select 
    emp.id emp_id, emp.orgdtl_id, emp.employment_class_id, emp.position_location_id, wk.id wk_id, inc.description, sum(ent.amount) amount
