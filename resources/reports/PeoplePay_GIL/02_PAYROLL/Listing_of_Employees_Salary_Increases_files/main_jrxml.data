    ent.admin_status = 'SYS'
    and inc.income_code = 'GAS'
    and (ent.end_date >= trunc(sysdate) or ent.end_date is null)
    and emp.org_id =  $P{IN_ORG_ID} 
    and std.id = $P{IN_SALARY_INCREASE_ID}
group by emp.id, emp.orgdtl_id, emp.employment_class_id, emp.position_location_id, wk.id, inc.description ) a
where a.orgdtl_id =  $P{IN_ORGDTL_ID}
and ($P{IN_ALL_EMPLOYEES} = 1 or
    	exists (SELECT 
    					1
    				FROM 
    					RPT_PARAMETER W1
    				WHERE 
    					W1.SESSION_ID =  $P{IN_SESSION_ID}
    				AND 
    					option_value=case when option_name = $P{IN_SEARCH_OPTION} and option_name ='EMPLOYEE' then to_char(a.emp_id)
						when option_name = $P{IN_SEARCH_OPTION} and option_name = 'ORG_STRUCT_LEVEL' then to_char(a.orgdtl_id)
						when option_name = $P{IN_SEARCH_OPTION} and option_name = 'EMPLOYMENT_CLASS' then to_char(a.employment_class_id)
                        when option_name = $P{IN_SEARCH_OPTION} and option_name = 'POSITION_LOCATION' then to_char(a.position_location_id)
                        when option_name = $P{IN_SEARCH_OPTION} and option_name = 'WORK_LOCATION' then to_char(a.wk_id)
						end
    				)
    )

order by a.orgdtl_id asc, a.description asc) b group by b.description]]>
