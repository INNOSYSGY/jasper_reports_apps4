	<parameter name="IN_ALL_EMPLOYEES" class="java.math.BigDecimal"/>
	<queryString language="SQL">
		<![CDATA[select 
    a.*,
    nvl(a.new_basic_pay, 0) + nvl(a.cumulative_allowance, 0) + nvl(a.telephone_allowance, 0) + 
    nvl(a.vehicle_allowance, 0) + nvl(a.meal_allowance, 0) + nvl(a.incentive, 0) + 
    nvl(a.entertainment_allowance, 0) + nvl(a.travel_allowance, 0) + nvl(a.gas_allowance, 0) gross_salary
from (
    select
        rownum no, 
        emp.id emp_id,
        ind.first_name, 
        ind.surname, 
        emp.emp_company_no,
        initcap(pos.position_name) position_name, 
        emp.date_employed,
        emp.employment_class_id, 
        emp.orgdtl_id,
        initcap(orgdtl.unit_name) unit_name, 
        emp.position_location_id,
        wk.id work_location_id,
        emp.org_id,
        sal.personal_rate - sal.inc_amount basic_pay,
        sal.inc_amount, 
        sal.personal_rate new_basic_pay,
        (select 
            ent.amount amount
        from
            hr_rcm_empentitle ent
            join pa_pcf_incomecode inc on ent.income_code_id = inc.id 
        where
            ent.admin_status = 'SYS'
            and inc.income_code = 'CUMMALL'
            and ent.emp_id = emp.id
            and (ent.end_date >= trunc(sysdate) or ent.end_date is null)) cumulative_allowance,
        (select 
            ent.amount amount
        from
            hr_rcm_empentitle ent
            join pa_pcf_incomecode inc on ent.income_code_id = inc.id 
        where
            ent.admin_status = 'SYS'
            and inc.income_code = 'TELALL'
            and ent.emp_id = emp.id and (ent.end_date >= trunc(sysdate) or ent.end_date is null)) telephone_allowance,
        (select 
            ent.amount amount
        from
            hr_rcm_empentitle ent
            join pa_pcf_incomecode inc on ent.income_code_id = inc.id 
        where
            ent.admin_status = 'SYS'
            and inc.income_code = 'VEHICLE'
            and ent.emp_id = emp.id and (ent.end_date >= trunc(sysdate) or ent.end_date is null)) vehicle_allowance,
        (select 
            ent.amount amount
        from
            hr_rcm_empentitle ent
            join pa_pcf_incomecode inc on ent.income_code_id = inc.id 
        where
            ent.admin_status = 'SYS'
            and inc.income_code = 'MEALS'
            and ent.emp_id = emp.id and (ent.end_date >= trunc(sysdate) or ent.end_date is null)) meal_allowance,
        (select 
            ent.amount amount
        from
            hr_rcm_empentitle ent
            join pa_pcf_incomecode inc on ent.income_code_id = inc.id 
        where
            ent.admin_status = 'SYS'
            and inc.income_code = 'INCEN'
            and ent.emp_id = emp.id and (ent.end_date >= trunc(sysdate) or ent.end_date is null)) incentive,
        (select 
            ent.amount amount
        from
            hr_rcm_empentitle ent
            join pa_pcf_incomecode inc on ent.income_code_id = inc.id 
        where
            ent.admin_status = 'SYS'
            and inc.income_code = 'ENT'
            and ent.emp_id = emp.id and (ent.end_date >= trunc(sysdate) or ent.end_date is null)) entertainment_allowance,
        (select 
            ent.amount amount
        from
            hr_rcm_empentitle ent
            join pa_pcf_incomecode inc on ent.income_code_id = inc.id 
        where
            ent.admin_status = 'SYS'
            and inc.income_code = 'TRAVEL'
            and ent.emp_id = emp.id and (ent.end_date >= trunc(sysdate) or ent.end_date is null)) travel_allowance,
        (select 
            ent.amount amount
        from
            hr_rcm_empentitle ent
            join pa_pcf_incomecode inc on ent.income_code_id = inc.id 
        where
            ent.admin_status = 'SYS'
            and inc.income_code = 'GAS'
            and ent.emp_id = emp.id and (ent.end_date >= trunc(sysdate) or ent.end_date is null)) gas_allowance
    from
        hr_rcm_individual ind
        left join hr_rcm_employee emp on ind.id = emp.ind_id
        left join hr_hcf_worklocationposition wkpos on emp.position_location_id = wkpos.id
        left join hr_hcf_worklocation wk on wk.id = wkpos.wklocation_id
        left join hr_hcf_position pos on pos.id = wkpos.position_id
        left join hr_hcf_orgstructuredtl orgdtl on orgdtl.id = pos.orgdtl_id
        join hr_rcm_salary sal on (sal.emp_id = emp.id and (sal.end_date is null or sal.end_date > trunc(sysdate)))
        join hr_apu_standardincrease std on std.id = sal.increase_id
        where emp.org_id = $P{IN_ORG_ID} 
        and std.id =  $P{IN_SALARY_INCREASE_ID}
        and (emp.date_separated is null or emp.date_separated >= trunc(sysdate))
        and ($P{IN_ALL_EMPLOYEES} =1 or
    	exists (SELECT 
    					1
    				FROM 
    					RPT_PARAMETER W1
    				WHERE 
    					W1.SESSION_ID = $P{IN_SESSION_ID}
    				AND 
    					option_value=case when option_name = $P{IN_SEARCH_OPTION} and option_name ='EMPLOYEE' then to_char(sal.emp_id)
						when option_name = $P{IN_SEARCH_OPTION} and option_name = 'ORG_STRUCT_LEVEL' then to_char(emp.orgdtl_id)
						when option_name =$P{IN_SEARCH_OPTION}and option_name = 'EMPLOYMENT_CLASS' then to_char(emp.employment_class_id)
                        when option_name = $P{IN_SEARCH_OPTION} and option_name = 'POSITION_LOCATION' then to_char(emp.position_location_id)
                        when option_name = $P{IN_SEARCH_OPTION} and option_name = 'WORK_LOCATION' then to_char(wk.id)
						end
    				)
    )
        ) a order by a.unit_name]]>
	</queryString>
