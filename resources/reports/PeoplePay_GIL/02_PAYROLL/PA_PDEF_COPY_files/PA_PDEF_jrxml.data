		<parameter name="P_EMP" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select c.DESCRIPTION ||'('|| d.DEDUCTION_NO||')' DESCB,
d.DEDUCTION_AMOUNT
from pa_pcf_admindeduction d
join pa_pcf_deductioncode c on d.expense_id = c.id
where d.PERIOD =0
and d.deduction_amount >0
and d.emp_id=$P{P_EMP}]]>
		</queryString>
		<field name="DESCB" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="DESCB"/>
		</field>
		<field name="DEDUCTION_AMOUNT" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="DEDUCTION_AMOUNT"/>
		</field>
	</subDataset>
	<parameter name="IN_ORG_ID" class="java.math.BigDecimal"/>
	<parameter name="emp_id" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[Select
 CASE WHEN UPPER(B.EMPLOYMENT_STATUS) = 'ACTIVE' THEN 'PAYROLL DATA ENTRY FORM'
 WHEN UPPER(B.SEPARATED_STATUS) IS NOT NULL THEN 'EMPLOYMENT SEPARATION FORM'
 END REPORT_TITLE,
 CASE WHEN UPPER(B.SEPARATED_STATUS) IS NOT NULL THEN B.SEPARATED_STATUS
 END  STATUS,
 Initcap(Surname) Surname,Initcap(First_Name) First_Name,Initcap(Middle_Name) Middle_Name,
b.id emp_id,
pkg_global_fnts.fn_Address('HOME', ind_id , 'ALL',sysdate) address
,A.Dob,Case When A.Sex_Code='F' Then 'Female' Else 'Male' End Sex_Code
,Marital_Code
,B.Position_Name
,B.Org_Structure
,M.address "Work Location"
,B.Date_Employed
,B.Date_Separated
, Pkg_Global_Fnts.Fn_Getpaymode(B.Id, sysdate) Payment_Mode
,Bank_Account_No
,(Select Organisation_Name From Hr_Hcf_Organisation Where Id=A.Bank_Branch_Id) Bank
,(select ADDRESS from HR_HCF_ORGANISATION where id=a.bank_branch_id) bank_address
,Pkg_Global_Fnts.Fn_Getcurnistaxno(A.Id , 'NIS NUMBER',  sysdate ) Nis_No
,Pkg_Global_Fnts.Fn_Getcurnistaxno(A.Id , 'TIN',  sysdate ) Tax_no
,Pkg_Global_Fnts.Fn_Getcurnistaxno(A.Id , 'NATIONAL ID',  sysdate ) ID_NO
,Pkg_Global_Fnts.Fn_Hourlyrate(b.id, sysdate ) Hourly_Rate
,Pkg_Global_Fnts.Fn_Personalrate(B.Id, sysdate ) Personal_Rate,
B.Payment_Type,
Pkg_Global_Fnts.Fn_Getemprule(B.Id, 'STANDARD_DAYS',sysdate) Req_Days_By_Period,
Pkg_Global_Fnts.Fn_Getemprule(B.Id, 'STANDARD_HOURS',sysdate) Req_Hours_By_Period
,EMPLOYMENT_CLASS
,B.Payment_Type AS PAYMENT
,(SELECT
sum(amount)
FROM VW_EMPENTITLE E
WHERE
TRUNC(SYSDATE) BETWEEN TRUNC(DATE_EFFECTIVE) AND TRUNC(NVL(DATE_EFFECTIVE_END,SYSDATE))
AND e.emp_id =  $P{emp_id} 
AND AMOUNT > 0) Total_Income
,UPPER(b.Entered_by) Entered_by,B.Entry_Date,UPPER(B.Last_Changed_By) Last_Changed_By,B.Last_Changed_Date,b.Emp_Company_No,
B.ORG_ID
From Hr_Rcm_Individual A Join Hr_Rcm_Employee B On A.Id=B.Ind_Id
join hr_hcf_organisation m on m.id=b.org_id
where org_id = $P{IN_ORG_ID} and b.id = $P{emp_id}]]>
	</queryString>
