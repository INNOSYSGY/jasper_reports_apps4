	<parameter name="IN_ORG_ID" class="java.math.BigDecimal"/>
	<parameter name="DEPARTMENT_ID" class="java.math.BigDecimal"/>
	<parameter name="EARNINGS_PERIOD_ID" class="java.math.BigDecimal"/>
	<parameter name="WKLOCATION_ID" class="java.math.BigDecimal"/>
	<parameter name="EMP_ID" class="java.math.BigDecimal"/>
	<queryString language="SQL">
		<![CDATA[select distinct tc.emp_id,
	   round(pkg_global_fnts.fn_hourlyrate(tc.emp_id, trunc(sysdate)),2) HOURLY_RATE,
	   round((pkg_global_fnts.fn_hourlyrate(tc.emp_id, trunc(sysdate)) * 1.5 ) * sum(nvl(tc.over_time,0)),2) T_OVERTIME, 
       round(nvl(AHOC_OVERTIME,0),2) AHOC_OVERTIME,
       round(pkg_global_fnts.fn_hourlyrate(tc.emp_id, trunc(sysdate)) * sum(nvl(tc.double_time,0)),2) T_DOUBLETIME,
       round(nvl(AHOC_DOUBLETIME,0),2) AHOC_DOUBLETIME,
       round(pkg_global_fnts.fn_hourlyrate(tc.emp_id, trunc(sysdate)) * sum(nvl(tc.total_time_lost, 0)),2) ABSENT_TIME,
       round(nvl(ABSENT_DAYS,0),2) ABSENT_DAYS,
       Initcap(ind.Surname) || ', ' || Initcap(ind.First_Name) || ' ' || nvl(Initcap(ind.Middle_Name),'') Name,
       tc.emp_company_no,
       tc.payment_type,
       emp.position_name "Position Name",
       dept.unit_name department,
       tc.designation,
       wkloc.location_description "Work Location",
        round(((pkg_global_fnts.fn_hourlyrate(tc.emp_id, trunc(sysdate)) * 1.5 ) * sum(nvl(tc.over_time,0))+
       	nvl(AHOC_OVERTIME,0) + nvl(AHOC_DOUBLETIME,0) +
       	pkg_global_fnts.fn_hourlyrate(tc.emp_id, trunc(sysdate)) * sum(nvl(tc.double_time,0)) ),0) TOTAL_OT_PAY,
       	round((pkg_global_fnts.fn_hourlyrate(tc.emp_id, trunc(sysdate)) * sum(nvl(tc.total_time_lost, 0)) +
       	nvl(ABSENT_DAYS,0)),0) TOTAL_DEDUCTION,
       	       	TO_CHAR(ep.start_date,'FMMonth DD, YYYY' ) ||' -- '|| TO_CHAR(ep.end_date, 'FMMonth DD, YYYY') 
        ||'   '|| initcap(PKG_GLOBAL_FNTS.Get_Lookup_Col(emp.employment_class_id, 'VALUE_DESCRIPTION' ) ||' '||tc.payment_type) PDATE
from hr_att_timeclock_archive tc
join hr_rcm_employee emp on emp.id = tc.emp_id
join pa_pcf_earningperiod ep on ep.id = tc.earnings_period_id
join hr_rcm_individual ind on emp.ind_id = ind.id
full join (  select distinct ah.emp_id,
            case when i.income_code = 'OT' then nvl(ah.amount_paid,0) else 0 end AHOC_OVERTIME,
            case when i.income_code = 'OT1' then nvl(ah.amount_paid,0) else 0 end AHOC_DOUBLETIME
            from pa_pcf_adhocpayment ah
            join hr_rcm_empentitle et on et.id = ah.empent_id
            join pa_pcf_incomecode i on i.id = et.income_code_id
            join hr_att_timeclock_upload t on t.earnings_period_id = ah.earnings_period_id
            where i.income_code in ('OT','OT1')
            group by i.income_code,ah.amount_paid,ah.emp_id,ah.quantity) ad on ad.emp_id = tc.emp_id
full join ( select distinct ad.emp_id,ad.closing_earning_period,
            case when Upper(dc.expense_code) = 'LESSDAYS' then nvl(ad.deduction_amount,0) else 0 end ABSENT_DAYS
            from pa_pcf_admindeduction ad 
            join pa_pcf_deductioncode dc on dc.id = ad.expense_id
            join hr_att_timeclock_upload cl on cl.earnings_period_id = ad.closing_earning_period
            where dc.expense_code = 'LESSDAYS'
            group by ad.emp_id,dc.expense_code,ad.deduction_amount,ad.closing_earning_period) ded on ded.emp_id = tc.emp_id
left outer join (select a.id,a.unit_name,wkloc.position_id,wkloc.id wkl
                        from  Hr_Hcf_Orgstructuredtl a
                        join  hr_hcf_position c on a.id= c.ORGDTL_ID
                        --join Table(Pkg_Global_Fnts.Get_Lookup_Value('TBLORGSTRUCTCLASS')) x on x.id=a.CLASSIFICATION
                        join VW_ORG_WORKLOCATION_POSITION wkloc on wkloc.POSITION_ID = c.id  
                        group by a.id,a.unit_name,wkloc.position_id,wkloc.id) dept on dept.wkl = emp.POSITION_LOCATION_ID
left outer join (select id, org_id, location_description
        					from hr_hcf_worklocation
        					group by id, org_id, location_description) wkloc on wkloc.id = tc.work_location_id
where tc.emp_id = nvl($P{EMP_ID},tc.emp_id) 
and emp.orgdtl_id = nvl($P{DEPARTMENT_ID},emp.orgdtl_id)
and tc.work_location_id = nvl($P{WKLOCATION_ID} ,tc.work_location_id)  
and tc.earnings_period_id = ded.closing_earning_period 
and tc.earnings_period_id = $P{EARNINGS_PERIOD_ID} 
and tc.org_id = $P{IN_ORG_ID} 
group by tc.emp_id,  Initcap(ind.Surname) || ', ' || Initcap(ind.First_Name) || ' ' || nvl(Initcap(ind.Middle_Name),''), 
tc.emp_company_no, tc.payment_type,emp.position_name, dept.unit_name, tc.designation, ep.start_date,
AHOC_OVERTIME,AHOC_DOUBLETIME,ABSENT_DAYS,wkloc.location_description,emp.employment_class_id,ep.end_date]]>
	</queryString>
