		<parameter name="IN_EMP_ID" class="java.math.BigDecimal"/>
		<queryString language="SQL">
			<![CDATA[select 
    ent_curr.emp_id, inc_curr.description, to_char(sum(ent_curr.amount), 'FML999G999G999G999G990D00') amount
from
    hr_rcm_empentitle ent_curr
    join pa_pcf_incomecode inc_curr on ent_curr.income_code_id = inc_curr.id 
where
    ent_curr.emp_id = $P{IN_EMP_ID} 
    and ent_curr.admin_status = 'SYS'
    and (ent_curr.end_date >= trunc(sysdate) or ent_curr.end_date is null)
    and inc_curr.income_code != 'CUMMALL'
    group by ent_curr.emp_id, inc_curr.description]]>
		</queryString>
		<field name="DESCRIPTION" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.name" value="DESCRIPTION"/>
			<property name="com.jaspersoft.studio.field.label" value="DESCRIPTION"/>
		</field>
		<field name="AMOUNT" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.name" value="AMOUNT"/>
			<property name="com.jaspersoft.studio.field.label" value="AMOUNT"/>
		</field>
	</subDataset>
	<parameter name="IN_ORG_ID" class="java.math.BigDecimal"/>
	<parameter name="IN_ALL_EMPLOYEES" class="java.math.BigDecimal"/>
	<parameter name="IN_SESSION_ID" class="java.math.BigDecimal"/>
	<parameter name="IN_SEARCH_OPTION" class="java.lang.String"/>
	<parameter name="IN_SALARY_INCREASE_ID" class="java.math.BigDecimal"/>
	<queryString language="SQL">
		<![CDATA[select
	d.normal_shift,
	d.normal_shift_duration,
	    case 
        when d.lunch_hour = 0.5 then ' less half hour lunch<br>' 
        when d.lunch_hour = 1 then ' less hour lunch<br>' 
        when d.lunch_hour = 2 then ' less two hour lunch<br>' 
        else '<br>' 
        end standard_lunch_time,
        case when  upper(d.position_name) like upper('%surveillance%') or upper(d.position_name) like upper('%guard%')  
                then ' hours × 6 days ' 
                else ' hours × 5 days ' end days_hours_worked,
    case when d.payment_type = 'MONTHLY' 
                then '= ' || d.normal_shift_duration * 5 || ' hours × 52 weeks ÷ 12 months'  
              when d.payment_type = 'WEEKLY' then null end monthly_weekly_hours_worked,
    case when  upper(d.position_name) like upper('%surveillance%') or upper(d.position_name) like upper('%guard%')  
            then 'Basic pay per week - '|| d.standard_hours || ' hours × ' || to_char(d.hourly_rate, 'FML999G999G999G999G990D00')  || ' per hour' 
            else  'Basic Salary ' || d.standard_hours || ' hours × ' || to_char(d.hourly_rate, 'FML999G999G999G999G990D00') 
            end standard_hours_worked,
    case when d.extra_hours is not null 
                then 'Extra Hours ' || d.extra_hours || '  × ' || to_char(d.hourly_rate, 'FML999G999G999G999G990D00') || ' × 1.5' 
            else null 
            end extra_hours_worked,
    d.saturday_shift,
    case when d.sat_less_lunch_hour = 1 then ' less hour lunch<br>' 
        else '<br>' 
        end saturday_lunch,
    case when d.payment_type = 'MONTHLY' then d.saturday_shift_duration ||  ' hours × 52 weeks ÷ 12 months <br>' 
        when d.payment_type = 'WEEKLY' then null
        end saturday_monthly_weekly_hours_worked,
    d.total_saturday_hours || ' hours × ' || to_char(d.hourly_rate, 'FML999G999G999G999G990D00') || ' × 1.5' saturday_extra_hours,
    case when d.payment_type = 'MONTHLY' then 'total salary of ' 
				when d.payment_type = 'WEEKLY' then 'total wage of ' 
			end salary_or_wage,
	to_char(case when  upper(d.position_name) like upper('%surveillance%') or upper(d.position_name) like upper('%guard%')  
        then d.total_salary - 1320 else d.total_salary end, 'FML999G999G999G999G990D00') || '(' || initcap(pkg_global_fnts.fn_num_towords(round(case when  upper(d.position_name) like upper('%surveillance%') or upper(d.position_name) like upper('%guard%')  then d.total_salary - 1320 else d.total_salary end))) || ' Dollars) ' salary_to_words,
    case when d.payment_type = 'MONTHLY' then 'Salary ' when d.payment_type = 'WEEKLY' then 'Wage ' end salary_wage,  
    
    case when trunc(sysdate) < add_months(d.date_employed, 3) then 'This will be three(3) months from the date of this letter, unless compelling reasons arise for an extension of this period. If your performance is excellent, this probationary period may be reduced. However, until a letter is issued, your position remains "on probation".' else null end probation_clause,
    case when upper(d.income_codes) like upper('%Attendance Incentive%') then 'Incentive is O-N-L-Y payable on full attendance of a six(6) days work week.' else null end att_inc,
    case when d.confirmed = 0 then 'On Confirmation you will receive a total sum of ' || to_char(round(d.total_salary + (d.total_salary * 0.06)), 'FML999G999G999G999G990D00') || ' (' || initcap(pkg_global_fnts.fn_num_towords(round(d.total_salary + (d.total_salary * 0.06))))  || ' Dollars). A new Letter of Appointment will be given stating the new rate of pay per hour.' else null end confirm_clause,
    case when upper(d.department) like upper('%Trading- Parika%') then '<b><u>Sundays</u><br><br>If you are required to work on Sundays:</b>' else null end parika_1, 
    case when upper(d.department) like upper('%Trading- Parika%') then '<b>a) You will be given one day off during the week. <br>b) For services rendered on Sundays, you shall be paid at the rate of pay plus 1.5.</b>' else null end parika_2, 
    case when  upper(d.position_name) like upper('%surveillance%') or upper(d.position_name) like upper('%guard%')  then 'Weekly hours' else null end cond_1,
    case when  upper(d.position_name) like upper('%surveillance%') or upper(d.position_name) like upper('%guard%')  then 'Overtime '|| d.monthly_hours ||' hours less ' ||d.standard_hours || ' =hours ' || round(d.monthly_hours - d.standard_hours) || ' × 1.5' else null end cond_2,
    case when  upper(d.position_name) like upper('%surveillance%') or upper(d.position_name) like upper('%guard%')  then 'Total hours to pay for ' else null end cond_3,    
    case when  upper(d.position_name) like upper('%surveillance%') or upper(d.position_name) like upper('%guard%')  then 'Cumulative overtime ' || d.extra_hours || ' hours × ' || to_char(d.hourly_rate, 'FML999G999G999G999G990D00') || ' per hour' else null end cond_4,
     case when  upper(d.position_name) like upper('%surveillance%') or upper(d.position_name) like upper('%guard%')  then 1 else 0 end show_frame,
    d.*,
    to_char(d.basic_salary) basic_salary_formatted,
    to_char(d.extra_hours_salary) extra_hours_salary_formatted,
    to_char(d.total_saturday_salary) total_saturday_salary_formatted,
    to_char(d.salary,'FML999G999G999G999G990D00') salary_formatted,
    to_char(d.total_salary) total_salary_formatted,
    d.date_effective,
    d.allow_cmlt_amount,
    d.allow_non_cmlt_amount,
    d.non_cmlt_description
from(
select 
    c.*,
    nvl(c.basic_salary, 0) + nvl(c.allow_cmlt_amount,0) salary,
    nvl(c.basic_salary, 0) + nvl(c.allow_cmlt_amount,0) + nvl(c.allow_non_cmlt_amount,0)  total_salary
from(
select 
    b.*, 
    case when  upper(b.position_name) like upper('%surveillance%') or upper(b.position_name) like upper('%guard%')  then round(b.monthly_hours - b.standard_hours, 3) * 1.5  else  round(b.monthly_hours - b.standard_hours, 3) end extra_hours,
    round(round(b.monthly_hours - b.standard_hours, 3) * b.hourly_rate * 1.5) extra_hours_salary,
    round(b.total_saturday_hours * b.hourly_rate * 1.5) total_saturday_salary
from(
select
    a.*,
    case when  upper(a.position_name) like upper('%surveillance%') or upper(a.position_name) like upper('%guard%')  then  round(case when a.payment_type = 'MONTHLY' then ((a.normal_shift_duration * 6) * 52)/12 when a.payment_type = 'WEEKLY' then (a.normal_shift_duration * 6) end, 2) else round(case when a.payment_type = 'MONTHLY' then ((a.normal_shift_duration * 5) * 52)/12 when a.payment_type = 'WEEKLY' then (a.normal_shift_duration * 5) end, 2) end monthly_hours,
    round(a.standard_hours * a.hourly_rate, 2) basic_salary,
    round(case when a.payment_type = 'MONTHLY' then (a.saturday_shift_duration * 52)/12 when a.payment_type = 'WEEKLY' then a.saturday_shift_duration end , 2) total_saturday_hours,
    case when upper(a.department) like upper('%Department%') then a.department else a.department || ' Department' end department_name
from (
    select
    to_char(trunc(sysdate), 'fmMonth fmDD, YYYY') date_prepared,
    (select ind.first_name ||' ' || ind.surname ||'<br>Personnel Officer'
from 
    HR_HCF_WORKLOCATIONPOSITION  wkp
    left join hr_hcf_position pos on wkp.position_id = pos.id
    left join hr_rcm_employee emp on emp.position_location_id = wkp.id
    left join hr_rcm_individual ind on ind.id = emp.ind_id
where 
    upper(pos.position_name) like upper('%Personnel Officer%')
    and (emp.date_separated is null or emp.date_separated > trunc(sysdate))
    and rownum = 1) signature,
    case when ind.sex_code = 'M' then 'Mr. ' else 'Ms. ' end || ind.first_name ||' ' || ind.surname name,
    case when ind.sex_code = 'M' then 'Mr. ' else 'Ms. ' end || ind.surname surname,
    (select position_name from hr_hcf_position where id = wkpos.position_id) position_name,
    (select unit_name from hr_hcf_position pos join hr_hcf_orgstructuredtl org on pos.orgdtl_id = org.id where pos.id = wkpos.position_id)  department,
    (select location_description from hr_hcf_worklocation where id = wkpos.wklocation_id) work_location,
    emp.payment_type,
    pkg_global_fnts.fn_Address('HOME', emp.ind_id , 'ALL',sysdate) ind_address,
    to_number(Pkg_Global_Fnts.Fn_Getemprule(emp.Id, 'STANDARD_HOURS',sysdate)) standard_hours,
    Pkg_Global_Fnts.Fn_Hourlyrate(emp.id, sysdate) Hourly_Rate,
    sal.start_date sal_start_date, emp.lunch_hour,
    (select to_char(start_time,'HH:MI PM')||' to '||to_char(end_time,'HH:MI PM') 
        from HR_ATT_SHIFT where id = emp.shift_id) normal_shift,
    round((select (24 * abs(END_TIME- START_TIME)) - nvl(emp.lunch_hour, 0)
        from hr_att_shift
        where id = emp.shift_id), 2) normal_shift_duration,
    (select to_char(start_time,'HH:MI PM')||' to '||to_char(end_time,'HH:MI PM')
        from HR_ATT_SHIFT a where is_saturday = 1 and id = emp.saturday_shift_id) saturday_shift,
    round((select (24 * abs(END_TIME- START_TIME)) - case when lunch_time_start is not null and lunch_time_end is not null then (24*abs(LUNCH_TIME_END- LUNCH_TIME_START)) else case when less_lunch_hour = 1 then 1 else 0 end end
        from hr_att_shift
        where is_saturday = 1 and id = emp.saturday_shift_id), 2) saturday_shift_duration,
    shift.less_lunch_hour less_lunch_hour,
    sat_shift.less_lunch_hour sat_less_lunch_hour, 
    ent.amount entitlements, 
    sal.personal_rate personal_rate,
    sal.emp_id emp_id, 
    emp.employment_class_id employment_class_id,
    emp.orgdtl_id orgdtl_id,
    emp.position_location_id position_location_id,
    wk.id work_location_id,
    emp.org_id org_id,
    std.id salary_increase_id,
    std.date_effective date_effective,
    std.new_salary_date,
    emp.date_employed, 
    inc_code.inc_desc income_codes,
    emp.confirmed,
    ent.cmlt_amount allow_cmlt_amount,
    ent.non_cmlt_amount allow_non_cmlt_amount,
    ent.non_cmlt_description non_cmlt_description,
        CASE
            WHEN EXISTS (
            SELECT 1
            FROM vw_salary_increase_approver d
            WHERE d.employee_no = emp.id
        ) THEN se.sig_photo
        ELSE null
    END AS sig_photo,
    se.increase_id se_increase_id
    from 
        hr_rcm_employee emp 
        left join hr_hcf_worklocationposition wkpos on emp.position_location_id = wkpos.id
        left join hr_hcf_worklocation wk on wk.id = wkpos.wklocation_id
        left join hr_att_shift shift on shift.id = emp.shift_id
        left join hr_att_shift sat_shift on sat_shift.id = emp.saturday_shift_id
        left join hr_rcm_individual ind on emp.ind_id = ind.id
        left join hr_rcm_address adr on (adr.ind_id = ind.id and upper(adr.address_type) = upper('HOME') and (adr.end_date is null or adr.end_date > trunc(sysdate)))
        left join (select increase_id, emp_id from hr_rcm_salary union select increase_id, emp_id from hr_rcm_empentitle) rec on rec.emp_id = emp.id
        left join hr_rcm_salary sal on (sal.emp_id = rec.emp_id and (sal.end_date is null or sal.end_date > trunc(sysdate)))
        left join hr_apu_standardincrease std on std.id = rec.increase_id
        join vw_salary_increase_approver se on se.increase_id = rec.increase_id
        left join (select 
        					ent_curr.emp_id,
        					sum(ent_curr.amount) amount,
        					case when inc_curr.income_code != 'CUMMALL' then inc_curr.description end non_cmlt_description,
        					case when inc_curr.income_code = 'CUMMALL' then  sum(ent_curr.amount) end cmlt_amount,
        					case when inc_curr.income_code != 'CUMMALL' then  sum(ent_curr.amount) end non_cmlt_amount
                        from
                            hr_rcm_empentitle ent_curr
                            join pa_pcf_incomecode inc_curr on ent_curr.income_code_id = inc_curr.id 
                        where
                            ent_curr.admin_status = 'SYS'
                            --and inc_curr.income_code = 'CUMMALL'
                            and (ent_curr.end_date >= trunc(sysdate) or ent_curr.end_date is null)
                            group by ent_curr.emp_id, inc_curr.income_code, inc_curr.description) ent on (ent.emp_id = emp.id)
         left join (select 
                            ent_curr.emp_id,  listagg(to_char(inc_curr.description), ';') within group (order by ent_curr.emp_id) inc_desc
                        from
                            hr_rcm_empentitle ent_curr
                            join pa_pcf_incomecode inc_curr on ent_curr.income_code_id = inc_curr.id 
                        where
                            ent_curr.admin_status = 'SYS'
                            and inc_curr.income_code != 'CUMMALL'
                            and (ent_curr.end_date >= trunc(sysdate) or ent_curr.end_date is null)
                            
                        group by ent_curr.emp_id) inc_code on inc_code.emp_id = emp.id
          
    where emp.org_id = $P{IN_ORG_ID}
    and std.id =$P{IN_SALARY_INCREASE_ID}
    and se.increase_id = $P{IN_SALARY_INCREASE_ID}
    and ($P{IN_ALL_EMPLOYEES} = 1 or
        exists (SELECT 
                        1
                    FROM 
                        RPT_PARAMETER W1
                    WHERE 
                        W1.SESSION_ID =$P{IN_SESSION_ID}
                    AND 
                        option_value=case when option_name =$P{IN_SEARCH_OPTION} and option_name ='EMPLOYEE' then to_char(sal.emp_id)
                        when option_name = $P{IN_SEARCH_OPTION} and option_name = 'ORG_STRUCT_LEVEL' then to_char(emp.orgdtl_id)
                        when option_name =$P{IN_SEARCH_OPTION} and option_name = 'EMPLOYMENT_CLASS' then to_char(emp.employment_class_id)
                        when option_name =$P{IN_SEARCH_OPTION} and option_name = 'POSITION_LOCATION' then to_char(emp.position_location_id)
                        when option_name =$P{IN_SEARCH_OPTION} and option_name = 'WORK_LOCATION' then to_char(wk.id)
                        end
                    )
    )
)a)b)c)d order by d.name asc]]>
	</queryString>
