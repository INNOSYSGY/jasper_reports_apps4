	<queryString language="SQL">
		<![CDATA[select
    d.*,
    to_char(round(d.old_basic_salary), 'FML999G999G999G999G990D00') old_basic_salary_formatted,
    to_char(round(d.confirm_basic_salary), 'FML999G999G999G999G990D00') confirm_basic_salary_formatted,
    to_char(round(d.extra_hours_salary), 'FML999G999G999G999G990D00') extra_hours_salary_formatted,
    to_char(round(d.extra_hours_confirm_salary), 'FML999G999G999G999G990D00') extra_hours_confirm_salary_formatted,
    to_char(round(d.total_saturday_salary), 'FML999G999G999G999G990D00') total_saturday_salary_formatted,
    to_char(round(d.total_confirm_saturday_salary), 'FML999G999G999G999G990D00') total_confirm_saturday_salary_formatted,
    to_char(round(d.old_salary), 'FML999G999G999G999G990D00') old_salary_formatted,
    to_char(round(d.confirm_salary), 'FML999G999G999G999G990D00') confirm_salary_formatted,
    to_char(round(d.old_total_salary), 'FML999G999G999G999G990D00') old_total_salary_formatted,
    to_char(round(d.confirm_total_salary), 'FML999G999G999G999G990D00') confirm_total_salary_formatted
from(
select 
    c.*,
    round(nvl(c.old_basic_salary, 0) + nvl(c.extra_hours_salary, 0) + case when  upper(c.position_name) like upper('%surveillance%') or upper(c.position_name) like upper('%guard%')  then 0 else nvl(c.total_saturday_salary,0) end) old_salary,
    round(nvl(c.confirm_basic_salary, 0) + nvl(c.extra_hours_confirm_salary, 0) + case when  upper(c.position_name) like upper('%surveillance%') or upper(c.position_name) like upper('%guard%')  then 0 else nvl(c.total_confirm_saturday_salary,0) end) confirm_salary,
    --nvl(c.personal_rate,0) + nvl(c.cumulative_allowance, 0) salary,
    --nvl(c.personal_rate,0) + nvl(c.cumulative_allowance, 0) + nvl(c.entitlements,0) total_salary
    round(nvl(c.old_basic_salary,0) + nvl(c.extra_hours_salary,0) + case when  upper(c.position_name) like upper('%surveillance%') or upper(c.position_name) like upper('%guard%')  then 0 else nvl(c.total_saturday_salary,0) end + nvl(c.entitlements,0)) old_total_salary,
    round(nvl(c.confirm_basic_salary,0) + nvl(c.extra_hours_confirm_salary,0) + case when  upper(c.position_name) like upper('%surveillance%') or upper(c.position_name) like upper('%guard%')  then 0 else nvl(c.total_confirm_saturday_salary,0) end + nvl(c.entitlements,0)) confirm_total_salary
from(
select 
    b.*, 
    case when  upper(b.position_name) like upper('%surveillance%') or upper(b.position_name) like upper('%guard%')  then round(b.monthly_hours - b.standard_hours, 2) * 1.5  else  round(b.monthly_hours - b.standard_hours, 2) end extra_hours,
    (round(b.monthly_hours - b.standard_hours, 2) * b.confirm_hourly_rate * 1.5) extra_hours_confirm_salary,
    (round(b.monthly_hours - b.standard_hours, 2) * b.hourly_rate * 1.5) extra_hours_salary,
    (b.total_saturday_hours * b.confirm_hourly_rate * 1.5) total_confirm_saturday_salary,
    (b.total_saturday_hours * b.hourly_rate * 1.5) total_saturday_salary
from(
select
    a.*,
    case when  upper(a.position_name) like upper('%surveillance%') or upper(a.position_name) like upper('%guard%')  then  round(case when a.payment_type = 'MONTHLY' then ((a.normal_shift_duration * 6) * 52)/12 when a.payment_type = 'WEEKLY' then (a.normal_shift_duration * 6) end, 2) else round(case when a.payment_type = 'MONTHLY' then ((a.normal_shift_duration * 5) * 52)/12 when a.payment_type = 'WEEKLY' then (a.normal_shift_duration * 5) end, 2) end monthly_hours,
    (a.standard_hours * a.confirm_hourly_rate) confirm_basic_salary,
    (a.standard_hours * a.hourly_rate) old_basic_salary,    
    round(case when a.payment_type = 'MONTHLY' then (a.saturday_shift_duration * 52)/12 when a.payment_type = 'WEEKLY' then a.saturday_shift_duration end , 2) total_saturday_hours,
    case when upper(a.department) like upper('%Department%') then a.department else a.department || ' Department' end department_name
from (
    select
    to_char(trunc(sysdate), 'fmMonth fmDD, YYYY') date_prepared,
    (select ind.first_name ||' ' || ind.surname ||'<br>Personnel Officer'
from 
    HR_HCF_WORKLOCATIONPOSITION  wkp
    left join hr_hcf_position pos on wkp.position_id = pos.id
    left join hr_rcm_employee emp on emp.position_location_id = wkp.id
    left join hr_rcm_individual ind on ind.id = emp.ind_id
where 
    upper(pos.position_name) like upper('%Personnel Officer%')
    and (emp.date_separated is null or emp.date_separated > trunc(sysdate))
    and rownum = 1) signature,
    case when ind.sex_code = 'M' then 'Mr. ' else 'Ms. ' end || ind.first_name ||' ' || ind.surname name,
    case when ind.sex_code = 'M' then 'Mr. ' else 'Ms. ' end || ind.surname surname,
    (select position_name from hr_hcf_position where id = wkpos.position_id) position_name,
    (select unit_name from hr_hcf_position pos join hr_hcf_orgstructuredtl org on pos.orgdtl_id = org.id where pos.id = wkpos.position_id)  department,
    (select location_description from hr_hcf_worklocation where id = wkpos.wklocation_id) work_location,
    emp.payment_type,
    pkg_global_fnts.fn_Address('HOME', emp.ind_id , 'ALL',sysdate) ind_address,
    to_number(Pkg_Global_Fnts.Fn_Getemprule(emp.Id, 'STANDARD_HOURS',sysdate)) standard_hours,
    Pkg_Global_Fnts.Fn_Hourlyrate(emp.id, sysdate) Hourly_Rate,
    sal.start_date sal_start_date, emp.lunch_hour,
    (select to_char(start_time,'HH:MI PM')||' to '||to_char(end_time,'HH:MI PM') 
        from HR_ATT_SHIFT where id = emp.shift_id) normal_shift,
    round((select (24 * abs(END_TIME- START_TIME)) - nvl(emp.lunch_hour, 0)
        from hr_att_shift
        where id = emp.shift_id), 2) normal_shift_duration,
    (select to_char(start_time,'HH:MI PM')||' to '||to_char(end_time,'HH:MI PM')
        from HR_ATT_SHIFT a where is_saturday = 1 and id = emp.saturday_shift_id) saturday_shift,
    round((select (24 * abs(END_TIME- START_TIME)) - case when lunch_time_start is not null and lunch_time_end is not null then (24*abs(LUNCH_TIME_END- LUNCH_TIME_START)) else case when less_lunch_hour = 1 then 1 else 0 end end
        from hr_att_shift
        where is_saturday = 1 and id = emp.saturday_shift_id), 2) saturday_shift_duration,
    shift.less_lunch_hour less_lunch_hour,
    sat_shift.less_lunch_hour sat_less_lunch_hour, 
    ent.amount entitlements, 
    sal.personal_rate personal_rate,
    sal.emp_id emp_id, 
    emp.employment_class_id employment_class_id,
    emp.orgdtl_id orgdtl_id,
    emp.position_location_id position_location_id,
    wk.id work_location_id,
    emp.org_id org_id,
    emp.date_employed, 
    inc_code.inc_desc income_codes,
    emp.confirmed, 
    emp.appointment_date date_confirmed,
    nvl(sal.confirm_hourly_rate,  Pkg_Global_Fnts.Fn_Hourlyrate(emp.id, sysdate)) confirm_hourly_rate,
    nvl(sal.confirm_personal_rate, sal.personal_rate) confirm_personal_rate,
    sal.confirm_start_date, 
    sal.confirm_end_date,
    to_char(nvl(sal.confirm_start_date, emp.appointment_date), 'fmMonth fmDD, YYYY') date_effective,
    emp.emp_company_no
    from 
        hr_rcm_employee emp 
        left join hr_hcf_worklocationposition wkpos on emp.position_location_id = wkpos.id
        left join hr_hcf_worklocation wk on wk.id = wkpos.wklocation_id
        left join hr_att_shift shift on shift.id = emp.shift_id
        left join hr_att_shift sat_shift on sat_shift.id = emp.saturday_shift_id
        left join hr_rcm_individual ind on emp.ind_id = ind.id
        left join hr_rcm_address adr on (adr.ind_id = ind.id and upper(adr.address_type) = upper('HOME') and (adr.end_date is null or adr.end_date > trunc(sysdate)))
        left join hr_rcm_salary sal on (sal.emp_id = emp.id and (sal.end_date is null or sal.end_date > trunc(sysdate)))
        left join (select 
                            ent_curr.emp_id, sum(ent_curr.amount) amount
                        from
                            hr_rcm_empentitle ent_curr
                            join pa_pcf_incomecode inc_curr on ent_curr.income_code_id = inc_curr.id 
                        where
                            ent_curr.admin_status = 'SYS'
                            and inc_curr.income_code != 'CUMMALL'
                            and (ent_curr.end_date >= trunc(sysdate) or ent_curr.end_date is null)
                            group by ent_curr.emp_id) ent on (ent.emp_id = emp.id)
        left join (select 
                            ent_curr.emp_id,  listagg(to_char(inc_curr.description), ';') within group (order by ent_curr.emp_id) inc_desc
                        from
                            hr_rcm_empentitle ent_curr
                            join pa_pcf_incomecode inc_curr on ent_curr.income_code_id = inc_curr.id 
                        where
                            ent_curr.admin_status = 'SYS'
                            and inc_curr.income_code != 'CUMMALL'
                            and (ent_curr.end_date >= trunc(sysdate) or ent_curr.end_date is null)
                        group by ent_curr.emp_id) inc_code on inc_code.emp_id = emp.id
    where emp.org_id = $P{IN_ORG_ID} 
    and (emp.date_separated is null or emp.separated_status_id is null)
    and ($P{IN_ALL_EMPLOYEES} = 1 or
    	exists (SELECT 
    					1
    				FROM 
    					RPT_PARAMETER W1
    				WHERE 
    					W1.SESSION_ID =$P{IN_SESSION_ID}
    				AND 
    					option_value=case when option_name =$P{IN_SEARCH_OPTION} and option_name ='EMPLOYEE' then to_char(sal.emp_id)
						when option_name = $P{IN_SEARCH_OPTION} and option_name = 'ORG_STRUCT_LEVEL' then to_char(emp.orgdtl_id)
						when option_name =$P{IN_SEARCH_OPTION} and option_name = 'EMPLOYMENT_CLASS' then to_char(emp.employment_class_id)
                        when option_name =$P{IN_SEARCH_OPTION} and option_name = 'POSITION_LOCATION' then to_char(emp.position_location_id)
                        when option_name =$P{IN_SEARCH_OPTION} and option_name = 'WORK_LOCATION' then to_char(wk.id)
						end
    				)
    )
)a)b)c)d order by d.name asc]]>
	</queryString>
