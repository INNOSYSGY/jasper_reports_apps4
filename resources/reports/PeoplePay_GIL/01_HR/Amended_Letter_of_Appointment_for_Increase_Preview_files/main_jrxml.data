	<queryString language="SQL">
		<![CDATA[select 
    'Management is pleased to inform you that you have been given an increase in salary with effect from ' || to_char(d.new_method_rate_date, 'fmMonth DD, YYYY') || ', under the following terms and conditions:-' par1, 
    '1. HOURS OF WORK:' heading1,
    'Mondays to Fridays '|| d.normal_shift || case when d.lunch_hour = 0.5 then ' less half hour lunch<br>' when d.lunch_hour = 1 then ' less hour lunch<br>' when d.lunch_hour = 2 then ' less two hour lunch<br>' else '<br>' end || d.normal_shift_duration || ' hours × 5 days ' || case when d.payment_type = 'MONTHLY' then '= ' || d.normal_shift_duration * 5 || ' hours × 52 weeks ÷ 12 months'  when d.payment_type = 'WEEKLY' then null end par1_1,
    'Basic Salary ' || d.standard_hours || ' hours × ' || to_char(d.new_hourly_Rate, 'FML999G999G999G999G990D00') par1_2,
    case when d.extra_hours is not null then 'Extra Hours ' || d.extra_hours || '  × ' || to_char(d.new_hourly_Rate, 'FML999G999G999G999G990D00') || ' × 1.5' else null end par1_3,
    'Saturdays - ' || d.saturday_shift  || case when d.sat_less_lunch_hour = 1 then ' less hour lunch<br>' else '<br>' end || case when d.payment_type = 'MONTHLY' then d.saturday_shift_duration ||  ' hours × 52 weeks ÷ 12 months <br>' when d.payment_type = 'WEEKLY' then null end || d.total_saturday_hours || ' hours × ' || to_char(d.new_hourly_Rate, 'FML999G999G999G999G990D00') || ' × 1.5' par1_4,
    '2. SALARY:' heading2,
    'You will receive a total salary of ' || to_char(round(d.total_salary), 'FML999G999G999G999G990D00') || '(' || initcap(pkg_global_fnts.fn_num_towords(d.total_salary)) || ') ' || case when d.payment_type = 'MONTHLY' then 'per month.' when d.payment_type = 'WEEKLY' then  'per week.' end par2_1, 
    'Salary would be subject to N.I.S, P.A.Y.E and other statutory deductions.' par2_2,
    'Because of the nature of your job you would be required to work beyond these stated hours from time to time to meet deadlines for which <u>overtime will be paid once authorized by your supervisor. However, you are to keep a document as copy attached for such overtime. Have your supervisor sign for such hours worked the same day. Without this document, no claim will be honored.</u>' par2_3,
    '3. OTHER TERMS & CONDITION:' heading3,
    'To remind you if you wish to resign you must give two(2) weeks’ notice after first six(6) months of employment and one(1) month’s notice over a year of employment or salary in lieu therefore.' par3_1,
    'All other terms and conditions of service remain the same as per your Letter of Appointment.' par3_2,
    d.*,
    to_char(round(d.basic_salary), 'FML999G999G999G999G990D00') basic_salary_formatted,
    to_char(round(d.extra_hours_salary), 'FML999G999G999G999G990D00') extra_hours_salary_formatted,
    to_char(round(d.total_saturday_salary), 'FML999G999G999G999G990D00') total_saturday_salary_formatted,
    to_char(round(d.salary), 'FML999G999G999G999G990D00') salary_formatted,
    to_char(round(d.total_salary), 'FML999G999G999G999G990D00') total_salary_formatted
from(
select 
    c.*,
    nvl(c.basic_salary, 0) + nvl(c.extra_hours_salary, 0) + nvl(c.total_saturday_salary,0) salary,
    --nvl(c.personal_rate,0) + nvl(c.cumulative_allowance, 0) salary,
    --nvl(c.personal_rate,0) + nvl(c.cumulative_allowance, 0) + nvl(c.entitlements,0) total_salary
    nvl(c.basic_salary,0) + nvl(c.extra_hours_salary,0) + nvl(c.total_saturday_salary, 0) + nvl(c.entitlements,0) total_salary
from(
select 
    b.*, 
    round(b.monthly_hours - b.standard_hours, 2) extra_hours,
    round(round(b.monthly_hours - b.standard_hours, 2) * b.new_hourly_rate * 1.5) extra_hours_salary,
    round(b.total_saturday_hours * b.new_hourly_rate * 1.5) total_saturday_salary
from(
select
    a.*,
    round(case when a.payment_type = 'MONTHLY' then ((a.normal_shift_duration * 5) * 52)/12 when a.payment_type = 'WEEKLY' then (a.normal_shift_duration * 5) end, 2) monthly_hours,
    round(a.standard_hours * a.new_hourly_rate, 2) basic_salary,
    round(case when a.payment_type = 'MONTHLY' then (a.saturday_shift_duration * 52)/12 when a.payment_type = 'WEEKLY' then a.saturday_shift_duration end , 2) total_saturday_hours,
    case when upper(a.department) like upper('%Department%') then a.department else a.department || ' Department' end department_name
from (
    select
    to_char(trunc(sysdate), 'fmMonth fmDD, YYYY') date_prepared,
    (select ind.first_name ||' ' || ind.surname ||'<br>Personnel Officer'
from 
    HR_HCF_WORKLOCATIONPOSITION  wkp
    left join hr_hcf_position pos on wkp.position_id = pos.id
    left join hr_rcm_employee emp on emp.position_location_id = wkp.id
    left join hr_rcm_individual ind on ind.id = emp.ind_id
where 
    upper(pos.position_name) like upper('%Personnel Officer%')
    and (emp.date_separated is null or emp.date_separated > trunc(sysdate))
    and rownum = 1) signature,
    case when ind.sex_code = 'M' then 'Mr. ' else 'Ms. ' end || ind.first_name ||' ' || ind.surname name,
    case when ind.sex_code = 'M' then 'Mr. ' else 'Ms. ' end || ind.surname surname,
    (select position_name from hr_hcf_position where id = wkpos.position_id) position_name,
    (select unit_name from hr_hcf_position pos join hr_hcf_orgstructuredtl org on pos.orgdtl_id = org.id where pos.id = wkpos.position_id)  department,
    (select location_description from hr_hcf_worklocation where id = wkpos.wklocation_id) work_location,
    emp.payment_type,
    pkg_global_fnts.fn_Address('HOME', emp.ind_id , 'ALL',sysdate) ind_address,
    Pkg_Global_Fnts.Fn_Getemprule(emp.Id, 'STANDARD_HOURS',sysdate) standard_hours,
    Pkg_Global_Fnts.Fn_Hourlyrate(emp.id, sysdate) Hourly_Rate,
    sal.start_date sal_start_date, emp.lunch_hour,
    (select to_char(start_time,'HH:MI PM')||' to '||to_char(end_time,'HH:MI PM') 
        from HR_ATT_SHIFT where id = emp.shift_id) normal_shift,
    round((select (24 * (END_TIME- START_TIME)) - nvl(emp.lunch_hour, 0)
        from hr_att_shift
        where id = emp.shift_id), 2) normal_shift_duration,
    (select to_char(start_time,'HH:MI PM')||' to '||to_char(end_time,'HH:MI PM')
        from HR_ATT_SHIFT a where is_saturday = 1 and id = emp.saturday_shift_id) saturday_shift,
    round((select (24 * (END_TIME- START_TIME)) - case when lunch_time_start is not null and lunch_time_end is not null then (24*(LUNCH_TIME_END- LUNCH_TIME_START)) else case when less_lunch_hour = 1 then 1 else 0 end end
        from hr_att_shift
        where is_saturday = 1 and id = emp.saturday_shift_id), 2) saturday_shift_duration,
    shift.less_lunch_hour less_lunch_hour,
    sat_shift.less_lunch_hour sat_less_lunch_hour, 
    ent.amount entitlements, 
    sal.personal_rate personal_rate,
    inc.hourly_rate new_hourly_rate,
    inc.new_method_rate_date
    from 
        hr_rcm_employee emp 
        left join hr_hcf_worklocationposition wkpos on emp.position_location_id = wkpos.id
        left join hr_att_shift shift on shift.id = emp.shift_id
        left join hr_att_shift sat_shift on sat_shift.id = emp.saturday_shift_id
        left join hr_rcm_individual ind on emp.ind_id = ind.id
        left join hr_rcm_address adr on (adr.ind_id = ind.id and upper(adr.address_type) = upper('HOME') and (adr.end_date is null or adr.end_date > trunc(sysdate)))
        left join hr_rcm_salary sal on (sal.emp_id = emp.id and (sal.end_date is null or sal.end_date > trunc(sysdate)))
        left join (select a.* from hr_apu_standardincreasevalues a join hr_apu_standardincrease b on a.increase_id = b.id where increase_id = $P{IN_INCREASE_ID}  and exists (select 1 from pa_pcf_incomecode ic where ic.id = a.income_code_id and org_id = $P{IN_ORG_ID}  and ic.income_code = 'NR')) inc on inc.emp_id = emp.id
        left join (select 
                            ent_curr.emp_id, sum(ent_curr.amount) amount
                        from
                            hr_rcm_empentitle ent_curr
                            join pa_pcf_incomecode inc_curr on ent_curr.income_code_id = inc_curr.id 
                        where
                            ent_curr.emp_id = $P{IN_EMP_ID} 
                            and ent_curr.admin_status = 'SYS'
                            and inc_curr.income_code != 'CUMMALL'
                            and (ent_curr.end_date >= trunc(sysdate) or ent_curr.end_date is null)
                            group by ent_curr.emp_id) ent on (ent.emp_id = emp.id)
    where emp.id = $P{IN_EMP_ID} 
    and emp.org_id =$P{IN_ORG_ID} 
)a)b)c)d]]>
	</queryString>
