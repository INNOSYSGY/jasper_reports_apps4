	<parameter name="IN_ORG_ID" class="java.math.BigDecimal"/>
	<parameter name="AS_OF_DATE" class="java.util.Date"/>
	<queryString language="SQL">
		<![CDATA[select
e.id emp_id, 
i.sex_code,
e.emp_company_no,
trim(i.Surname) || ', ' || trim(i.First_Name) || ' ' || nvl(trim(i.Middle_Name),'') Full_Name,
e.employment_class,
e.payment_type,
e.compute_gross,
(select value_description 
 from hr_hcf_lookup 
 where org_id =  $P{IN_ORG_ID}     and table_name = 'TBLEMPLOYMENTTYPE' and table_value = e.employment_type_id
) employment_type,

date_employed,date_separated,
O.ORGANISATION_NAME,OD.UNIT_NAME DEPARTMENT_NAME,P.POSITION_NAME,
pg.grade_code,pg.grade_mini,pg.grade_max,
s.personal_rate,
s.grade_point,
s.hourly_rate,
s.daily_rate,
sr.shift_cycle_name,
sr.hours_worked,
sr.num_working_days,

nvl(a. sum_allowances ,0) sum_allowances,
s.personal_rate + nvl(a.sum_allowances ,0) total_personal_rate,
round(   (s.personal_rate + nvl(a.sum_allowances ,0)) /sr.hours_worked ,2) compound_hourly_rate,

code_all.code_type,
code_all.deduction_type,
code_all.code_start_date,
code_all.code_end_date,
code_all.code_name,
code_all.code_desc,
code_all.code_amount,
code_all.code_status

from hr_rcm_employee e
join hr_rcm_individual i on i.id = e.ind_id
join hr_hcf_positiongrade pg on e.emp_grade_id = pg.id
left join hr_att_shift_rota sr on e.shift_id = sr.id and sr.org_id = e.org_id
left join (
				select s1.* 
				from hr_rcm_salary s1
                where trunc($P{AS_OF_DATE}) between s1.start_date and nvl( s1.end_date,trunc(sysdate)) 
				) s on s.emp_id = e.id
 left join (
                select ee1.emp_id,sum(ee1.amount)  sum_allowances
                from hr_rcm_empentitle ee1
                where ee1.admin_status = 'SYS'
                and trunc($P{AS_OF_DATE}) between ee1.start_date and nvl( ee1.end_date,trunc(sysdate)) 
                group by ee1.emp_id
                )  a on a.emp_id = e.id         
left join (
				select 'ALLOW' code_type, 'EMPLOYEE'  deduction_type, ee1.emp_id,ee1.start_date as code_start_date, ee1.end_date as code_end_date,
                            ic.income_code as code_name,ic.description as code_desc , ee1.amount as code_amount,cast(income_status as nvarchar2(10)) code_status
				from hr_rcm_empentitle ee1 join pa_pcf_incomecode ic on ee1.income_code_id = ic.id
				where ee1.admin_status = 'SYS'
				and trunc($P{AS_OF_DATE}) between ee1.start_date and nvl( ee1.end_date,trunc(sysdate)) 
				UNION
				select 'DEDUCT' code_type,  ad1.deduction_type, ad1.emp_id,ad1.datestart as code_start_date, ad1.date_close as code_end_date, 
                            dc.expense_code as code_name,dc.description as code_desc, deduction_amount as code_amount ,cast(deduct_status as nvarchar2(10)) code_status
				from pa_pcf_admindeduction ad1 join pa_pcf_deductioncode dc on ad1.expense_id = dc.id
				where dc.expense_code = 'PENSION'
				and trunc($P{AS_OF_DATE}) between ad1.datestart and nvl( ad1.date_close,trunc(sysdate)) 
				) code_all on code_all.emp_id = e.id		
left JOIN hr_hcf_position P ON e.position_id = P.ID
left JOIN HR_HCF_ORGSTRUCTUREDTL OD ON E.ORGDTL_ID = OD.ID
left JOIN HR_HCF_ORGANISATION O ON E.ORG_ID = O.ID
where trunc($P{AS_OF_DATE}) between e.date_employed and nvl(e.date_separated,trunc(sysdate))
and e.org_id = $P{IN_ORG_ID}    

and exists (
					  select 1
			          from HR_HCF_ORGSTRUCTUREDTL T left outer join VW_USERORGSTRUCT k on ( t.id = k.ORG_STRUCTURE_ID  and nvl(k.user_name,'1') = decode(k.user_name, null, '1', null))
			          where  t.id = e.ORGDTL_ID
			          and e.ORGDTL_ID=decode(pkg_security.fn_testfinegradeOrgStruct((select user_name from tbluser where employee_no = e.emp_company_no)), 0, e.ORGDTL_ID, k.org_structure_id)
				     )]]>
	</queryString>
