	<parameter name="IN_ORG_ID" class="java.math.BigDecimal"/>
	<parameter name="START_DATE" class="java.lang.String"/>
	<parameter name="END_DATE" class="java.lang.String"/>
	<parameter name="EMP_ID" class="java.math.BigDecimal"/>
	<parameter name="DEPARTMENT_ID" class="java.math.BigDecimal"/>
	<queryString language="SQL">
		<![CDATA[select ch.id,
ch.emp_id,
emp.emp_company_no,
ch.work_date,
'D'||lpad(EXTRACT(DAY FROM ch.work_date),2,'0') day_numberc, 
to_char(ch.work_date,'MM/DD') day_short,
Initcap(i.Surname) || ', ' || Initcap(i.First_Name) || ' ' || nvl(Initcap(i.Middle_Name),'') NAME,
emp.wklocation_id,
ch.orgdtl_id,
od.unit_name AS department,
p.position_name as designation,
emp.payment_type,
emp.employment_class,
lk.value_description shift_type_description,
ta.absence_hours,
pkg_global_fnts.get_lookup_col(ta.leave_type, 'TABLE_VALUE') abs_code
from hr_att_empcalendar_hd ch 
join hr_rcm_employee emp on emp.id = ch.emp_id
join hr_rcm_individual i on emp.ind_id = i.id
left join  hr_att_timesheetabsence ta on ch.emp_id = ta.emp_id and trunc(ch.work_date) = trunc(ta.work_date)
left join hr_att_shift s on ch.assigned_shift = s.id
left join hr_hcf_lookup lk on lk.table_name = 'TBLSHIFTTYPE' and lk.org_id = $P{IN_ORG_ID}
left join hr_hcf_position p on emp.position_id = p.id
left join hr_hcf_orgstructuredtl od on emp.orgdtl_id = od.id
where ch.orgdtl_id =  nvl( $P{DEPARTMENT_ID} , ch.orgdtl_id) 
	and ch.work_date between  trunc(to_date($P{START_DATE})) and trunc(to_date($P{END_DATE}))
	and ch.org_id = $P{IN_ORG_ID}]]>
	</queryString>
