	<queryString>
		<![CDATA[select b.ndc_name||' - Lot '||initcap(a.lot)||' '||initcap(a.street)||' '||
(Select r.village_name from ndc_village  r where r.id = a.ndc_id) address,
a.id property_id,
a.property_number,
f.owner_name,

(select ref_value from ref z where z.id = d.property_class) property_class
from property a join ndc b on b.id = a.ndc_id
join property_valuation c on c.property_id = a.id
join property_valuation_detail d on d.property_valuation_id = c.id
join property_owner e on e.property_id = a.id
join owner f on f.id = e.owner_id
where c.effective_end is null]]>
	</queryString>
