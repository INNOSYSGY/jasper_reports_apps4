	<subDataset name="Other Qualifications" uuid="5671c0f7-f14e-46dd-961e-7ca12e25dd5f">
		<parameter name="P_IND_ID" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
q.ind_id,
Q.Q_LEVEL,Q.COUNTRY_CODE,
Initcap(Q.SUBJECT_COURSE)SUBJECT_COURSE,
Q.GRADE_ATTAINED,
Q.Q_YEAR,
initcap(Q.EXAM_BODY)EXAM_BODY
FROM HR_RCM_QUALIFICATION Q
Where q.ind_id=$P{P_IND_ID}
and Q.Q_LEVEL not in ('CXC','CAPE','GCEA','GCEO','Degree','Masters','Phd','Postgrad')
ORDER BY Q.Q_YEAR desc]]>
		</queryString>
		<field name="IND_ID" class="java.math.BigDecimal"/>
		<field name="Q_LEVEL" class="java.lang.String"/>
		<field name="COUNTRY_CODE" class="java.lang.String"/>
		<field name="SUBJECT_COURSE" class="java.lang.String"/>
		<field name="GRADE_ATTAINED" class="java.lang.String"/>
		<field name="Q_YEAR" class="java.lang.String"/>
		<field name="EXAM_BODY" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Previous Employer" uuid="18f1cdf2-8744-4060-b067-38cae73beb0c">
		<parameter name="P_IND_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select

    initcap(hr_rcm_emphistory.organisation) as organisation,
    initcap(hr_rcm_emphistory.position_held) as position_held,
    initcap(HR_RCM_EMPHISTORY.DEPARTMENT) as DEPARTMENT,
    HR_RCM_EMPHISTORY.REASON_FOR_LEAVING as REASON_FOR_LEAVING,
    HR_RCM_EMPHISTORY.PREVIOUS_SALARY as PREVIOUS_SALARY,
    HR_RCM_EMPHISTORY.FROM_DATE as FROM_DATE,
    HR_RCM_EMPHISTORY.TO_DATE as TO_DATE

 from hr_rcm_emphistory hr_rcm_emphistory
where hr_rcm_emphistory.ind_id=$P{P_IND_ID}
order by  FROM_DATE desc]]>
		</queryString>
		<field name="ORGANISATION" class="java.lang.String"/>
		<field name="POSITION_HELD" class="java.lang.String"/>
		<field name="DEPARTMENT" class="java.lang.String"/>
		<field name="REASON_FOR_LEAVING" class="java.lang.String"/>
		<field name="PREVIOUS_SALARY" class="java.math.BigDecimal"/>
		<field name="FROM_DATE" class="java.sql.Timestamp"/>
		<field name="TO_DATE" class="java.sql.Timestamp"/>
	</subDataset>
	<subDataset name="Employee Training" uuid="bad8cf22-b397-4c1c-94a1-f7b668ed9ca2">
		<parameter name="EMP_NUM" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT DISTINCT * FROM (
SELECT
T.EMP_ID,
T.Q_LEVEL,
t.tutor,
INITCAP(T.SUBJECT_COURSE) SUBJECT_COURSE,
INITCAP(O.ORGANISATION_NAME) TRAINING_AGENCY,
--INITCAP(s.ORGANISATION_NAME) SPONSORING_AGENCY,
initcap(T.COUNTRY_CODE) Country,
t.train_start_date
FROM HR_RCM_TRAINING T
LEFT OUTER JOIN VW_ORGANISATION O ON T.TRAINING_AGENCY=O.ID
--left outer join VW_ORGANISATION s ON T.SPONSORING_AGENCY=O.ID
where t.emp_id=$P{EMP_NUM}
order by t.train_start_date desc)
where rownum <= 5
order by train_start_date desc]]>
		</queryString>
		<field name="EMP_ID" class="java.math.BigDecimal"/>
		<field name="Q_LEVEL" class="java.lang.String"/>
		<field name="TUTOR" class="java.lang.String"/>
		<field name="SUBJECT_COURSE" class="java.lang.String"/>
		<field name="TRAINING_AGENCY" class="java.lang.String"/>
		<field name="COUNTRY" class="java.lang.String"/>
		<field name="TRAIN_START_DATE" class="java.sql.Timestamp"/>
	</subDataset>
	<subDataset name="Referrences" uuid="6781b238-20c4-40bf-beb6-69881b9d5b5e">
		<parameter name="P_IND_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
    (SELECT case when upper(sex_code)='F' then 'Ms. ' else 'Mr. ' end ||SURNAME||', '||NVL(FIRST_NAME,'') FROM HR_RCM_INDIVIDUAL WHERE ID=HR_RCM_REFERENCE.IND_ID) as IND_ID,
        HR_RCM_REFERENCE.REMARKS as REMARKS

 from HR_RCM_REFERENCE HR_RCM_REFERENCE
where HR_RCM_REFERENCE.REFEREE_FOR =$P{P_IND_ID}
order by REF_DATE desc]]>
		</queryString>
		<field name="IND_ID" class="java.lang.String"/>
		<field name="REMARKS" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Employment Travel" uuid="a6295015-fc4f-4954-a452-a2ee8335b283">
		<parameter name="P_EMP_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
    initcap(HR_RCM_TRAVELHISTORY.DESTINATION) as DESTINATION,
    HR_RCM_TRAVELHISTORY.PURPOSE as PURPOSE,
    HR_RCM_TRAVELHISTORY.TRAVEL_DATE as TRAVEL_DATE,
    HR_RCM_TRAVELHISTORY.RETURN_DATE as RETURN_DATE
from HR_RCM_TRAVELHISTORY HR_RCM_TRAVELHISTORY
where hr_rcm_travelhistory.emp_id=$P{P_EMP_ID}
ORDER BY HR_RCM_TRAVELHISTORY.TRAVEL_DATE  DESC]]>
		</queryString>
		<field name="DESTINATION" class="java.lang.String"/>
		<field name="PURPOSE" class="java.lang.String"/>
		<field name="TRAVEL_DATE" class="java.sql.Timestamp"/>
		<field name="RETURN_DATE" class="java.sql.Timestamp"/>
	</subDataset>
	<subDataset name="Items Held" uuid="f8912da7-ea4e-4a87-b18a-bd1d84fc1935">
		<parameter name="P_EMP_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
    hr_rcm_empitem.item_code as item_code,
    (
    select value_description a
    from table(pkg_global_fnts.get_lookup_value('TBLITEM'))
   where id=hr_rcm_empitem.item_code

    ) item_description,
    HR_RCM_EMPITEM.START_PERIOD as START_PERIOD,
    HR_RCM_EMPITEM.END_PERIOD as END_PERIOD,
    HR_RCM_EMPITEM.PURPOSE as PURPOSE
 from HR_RCM_EMPITEM HR_RCM_EMPITEM
where HR_RCM_EMPITEM.EMP_ID =$P{P_EMP_ID}
ORDER BY  HR_RCM_EMPITEM.START_PERIOD  desc]]>
		</queryString>
		<field name="ITEM_CODE" class="java.lang.String"/>
		<field name="ITEM_DESCRIPTION" class="java.lang.String"/>
		<field name="START_PERIOD" class="java.sql.Timestamp"/>
		<field name="END_PERIOD" class="java.sql.Timestamp"/>
		<field name="PURPOSE" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Experience over the past 10 years" uuid="63900653-ec77-4a3b-a9ee-896b919df87d">
		<parameter name="P_EMP_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
  null dept,
  null period,
  null job_grade,
  null job_title_function
 from dual]]>
		</queryString>
		<field name="DEPT" class="java.lang.String"/>
		<field name="PERIOD" class="java.lang.String"/>
		<field name="JOB_GRADE" class="java.lang.String"/>
		<field name="JOB_TITLE_FUNCTION" class="java.lang.String"/>
	</subDataset>
	<parameter name="EMP_NO" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT DATE_SEPARATED,
SEPARATED_STATUS,
I.ID IND_ID,
e.id emp_id,
    (NVL(I.SURNAME,'') || ', ' ||NVL(I.FIRST_NAME,'')||' '||NVL(I.MIDDLE_NAME,'')) AS ENAME,
    INITCAP(I.SURNAME) SURNAME,
    INITCAP(I.FIRST_NAME) FIRST_NAME,
    initcap(I.MIDDLE_NAME) Middle_name,
    I.DOB,
    case when I.SEX_CODE='M' then 'Male' else 'Female' end as sex_code,
    E.DATE_EMPLOYED,
    E.EMP_COMPANY_NO,
    initcap(e.Employment_status) Employment_status ,
    UPPER(PKG_GLOBAL_FNTS.FN_GETCURNISTAXNO(E.IND_ID,'NIS NUMBER',SYSDATE)) NIS_NO,
    UPPER(PKG_GLOBAL_FNTS.FN_GETCURNISTAXNO(E.IND_ID,'TIN',SYSDATE)) TAX_NO,
    UPPER(PKG_GLOBAL_FNTS.FN_GETCURNISTAXNO(E.IND_ID,'NATIONAL ID',SYSDATE)) NATIONAL_ID,
  (Select INITCAP(P.Position_name)
     From Vw_Org_Worklocation_Position P
     Join Hr_Hcf_Worklocation Lp On  P.Wklocation_Id=Lp.Id
     where P.Orgdtl_Id=E.Orgdtl_Id and p.id=e.position_location_id) POSITION_NAME,
      (Select INITCAP(LP.LOCATION_DESCRIPTION)
     From Vw_Org_Worklocation_Position P
     Join Hr_Hcf_Worklocation Lp On  P.Wklocation_Id=Lp.Id
     where P.Orgdtl_Id=E.Orgdtl_Id and p.id=e.position_location_id) work_location,
            (Select Initcap(Unit_Name)
            from Vw_Organisation_Structure s where s.id=e.orgdtl_id) Department,
    initcap(E.EMPLOYMENT_CLASS) EMPLOYMENT_CLASS,
    initcap(Pkg_Global_Fnts.Fn_Address('HOME',I.Id,'LOT',Sysdate)||' '||PKG_GLOBAL_FNTS.FN_ADDRESS('HOME',I.ID,'STREET',SYSDATE)) LOT_STREET,
    initcap(PKG_GLOBAL_FNTS.FN_ADDRESS('HOME',I.ID,'VILLAGE_WARD',SYSDATE)) VILAGE_WARD,
    initcap(PKG_GLOBAL_FNTS.FN_ADDRESS('HOME',I.ID,'COUNTY',SYSDATE)) COUNTY,
Pkg_Global_Fnts.Fn_Address('HOME',I.Id,'REGION',Sysdate) Region,
pkg_global_fnts.fn_personalrate(e.id,sysdate) personal_rate,
initcap(e.payment_type)payment_type,
e.appointment_date,
email,
cell_phone,
telephone,
    HO.ADDRESS ORG_ADDRESS, HO.FAXNUM, HO.TELNUM, NIS_REG_NO, TAX_REG_NO,
ho.organisation_name,ho.photo org_photo,e.org_id,
fn_cxcgrade1s(i.id)+fn_cxcgrade2s(i.id)+fn_cxcgrade3s(i.id) || ' subject(s) - ' || decode(fn_cxcgrade1s(i.id),0,null,fn_cxcgrade1s(i.id)||' one(s) ')||decode(fn_cxcgrade2s(i.id),0,null,fn_cxcgrade2s(i.id)||' two(s) ')||decode(fn_cxcgrade3s(i.id),0,null,fn_cxcgrade3s(i.id)||' three(s) ') cxc,
fn_qualgrade(i.id,'CAPE') || ' subject(s)' CAPE,
fn_qualgrade(i.id,'GCEO') ||' subject(s)' O_LEVEL,
fn_qualgrade(i.id,'GCEA') ||' subject(s)' A_LEVEL,
fn_qualgrade(i.id,'Postgrad')+fn_qualgrade(i.id,'Degree')+fn_qualgrade(i.id,'MBA')
+fn_qualgrade(i.id,'Masters')+fn_qualgrade(i.id,'Phd')||' course(s)' UNI

    FROM HR_RCM_INDIVIDUAL I
    INNER JOIN  HR_RCM_EMPLOYEE E ON I.ID = E.IND_ID
    JOIN HR_HCF_ORGANISATION HO on e.org_id=ho.id
    WHERE
    (SEPARATED_STATUS IS NULL
    or (NVL(DATE_SEPARATED,SYSDATE+1)>SYSDATE))
   AND e.id=$P{EMP_NO}
     order by (NVL(I.SURNAME,'') || ', ' ||NVL(I.FIRST_NAME,'')||' '||NVL(I.MIDDLE_NAME,''))]]>
	</queryString>
	<field name="DATE_SEPARATED" class="java.sql.Timestamp"/>
	<field name="SEPARATED_STATUS" class="java.lang.String"/>
	<field name="IND_ID" class="java.math.BigDecimal"/>
