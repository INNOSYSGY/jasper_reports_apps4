cross join
(   select ORGANISATION_NAME,Address
    from hr_hcf_organisation b
    where b.id=$P{IN_ORG_ID}
) z
where a.earnings_period_id=$P{EARNINGS_PERIOD_ID}
and a.org_id=$P{IN_ORG_ID}
order by pname]]>
