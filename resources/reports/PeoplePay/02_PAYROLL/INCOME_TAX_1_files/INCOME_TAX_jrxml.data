	<parameter name="ORG_ID" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_YEAR" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="P_PAYMENT_TYPE" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
ho.id org_id,
ep.emp_company_no,
min(pa.period_start) Start_date,
max(pa.period_start) End_date,
nvl(pa.surname_hist,'')||', '||nvl(pa.first_name_hist,'')||' '||nvl(r.middle_name,'') employee_name,
initcap(nvl(r.surname,'')||', '||nvl(r.first_name,'')||' '||nvl(r.middle_name,''))||CHR(10)||initcap(
(Pkg_Global_Fnts.Fn_Address('HOME',r.Id,'LOT',TRUNC(SYSDATE))||' '||PKG_GLOBAL_FNTS.FN_ADDRESS('HOME',r.ID,'STREET',TRUNC(SYSDATE)))||chr(10)
||decode(PKG_GLOBAL_FNTS.FN_ADDRESS('HOME',r.ID,'VILLAGE_WARD',TRUNC(SYSDATE)),null,'',
PKG_GLOBAL_FNTS.FN_ADDRESS('HOME',r.ID,'VILLAGE_WARD',TRUNC(SYSDATE))||chr(10))
||nvl(PKG_GLOBAL_FNTS.FN_ADDRESS('HOME',r.ID,'COUNTY',TRUNC(SYSDATE)),' ')
)
EMPLOYEE_ADDRESS,
to_char(pa.period_start,'yyyy')+1 Return_Year,
UPPER(PKG_GLOBAL_FNTS.FN_GETCURNISTAXNO(Ep.IND_ID,'TIN',SYSDATE)) TAX_NO,
UPPER(PKG_GLOBAL_FNTS.FN_GETCURNISTAXNO(Ep.IND_ID,'NATIONAL ID',SYSDATE)) NATIONAL_ID,
UPPER(PKG_GLOBAL_FNTS.FN_GETCURNISTAXNO(Ep.IND_ID,'NIS NUMBER',SYSDATE)) NIS_NO,
pkg_global_fnts.fn_taxceiling(pa.org_id,to_char(pa.period_start,'yyyy'),upper(pa.payment_type_hist)) Threshold,
       sum(total_tax_earnings)- (sum(nvl(pkg_global_fnts.fn_lessAbs1(pa.period_start,pa.emp_id),0)))   total_tax_earnings,
sum(total_tax_earnings) Total_income,
       sum(incentives_taxable) incentives_taxable,
         sum(benefit_taxable) benefit_taxable,
       sum(allowance_taxable) allowance_taxable,
       sum(allowance_nontaxable) allowance_nontaxable,
      sum(pa.gross_taxable)- (sum(nvl(pkg_global_fnts.fn_lessAbs1(pa.period_start,pa.emp_id),0)))   as Total_Earning,
      sum(round(tax_amount + retro_taxes,0))    tax_amount,
sum(FREEPAY) FREEPAY,
sum(NIS_EMPLOYEE) NIS_EMPLOYEE,

nvl((
      select ROUND(sum(c.INCOME_AMOUNT),2) INCOME_AMOUNT
      from PA_PMG_PAYROLLINCOME c
      join pa_pmg_payrolldtl pd on c.pay_slipno=pd.id
      JOIN PA_PCF_INCOMECODE IC ON c.INCOME_CODE = IC.INCOME_CODE
      WHERE UPPER(c.INCOME_CODE) IN ('OT')
      and to_char(pd.period_start,'yyyy')=$P{P_YEAR}
      and pd.emp_id=pa.emp_id
),0) Overtime,
      HO.organisation_name,
ho.address,ho.faxnum,ho.telnum,nis_reg_no,tax_reg_no

from pa_pmg_payrolldtl pa
join VW_EMPLOYEE ep on pa.emp_id=ep.id
join HR_RCM_INDIVIDUAL r on ep.ind_id=r.id
join hr_hcf_organisation ho on pa.org_id=ho.id
where
pa.org_id=$P{ORG_ID}
and to_char(pa.period_start,'yyyy')=$P{P_YEAR}
and upper(trim(pa.payment_type_hist))=UPPER($P{P_PAYMENT_TYPE})
group by
ho.id,
pa.emp_id, ep.emp_company_no,pkg_global_fnts.fn_taxceiling(pa.org_id,to_char(pa.period_start,'yyyy'),upper(pa.payment_type_hist)),
nvl(pa.surname_hist,'')||', '||nvl(pa.first_name_hist,'')||' '||nvl(r.middle_name,''),
to_char(pa.period_start,'yyyy')+1
,UPPER(PKG_GLOBAL_FNTS.FN_GETCURNISTAXNO(Ep.IND_ID,'TIN',SYSDATE)),
UPPER(PKG_GLOBAL_FNTS.FN_GETCURNISTAXNO(Ep.IND_ID,'NATIONAL ID',SYSDATE)),
UPPER(PKG_GLOBAL_FNTS.FN_GETCURNISTAXNO(Ep.IND_ID,'NIS NUMBER',SYSDATE)),
HO.organisation_name,
ho.address,ho.faxnum,ho.telnum,nis_reg_no,tax_reg_no,
initcap(nvl(r.surname,'')||', '||nvl(r.first_name,'')||' '||nvl(r.middle_name,''))||CHR(10)||initcap(
(Pkg_Global_Fnts.Fn_Address('HOME',r.Id,'LOT',TRUNC(SYSDATE))||' '||PKG_GLOBAL_FNTS.FN_ADDRESS('HOME',r.ID,'STREET',TRUNC(SYSDATE)))||chr(10)
||decode(PKG_GLOBAL_FNTS.FN_ADDRESS('HOME',r.ID,'VILLAGE_WARD',TRUNC(SYSDATE)),null,'',
PKG_GLOBAL_FNTS.FN_ADDRESS('HOME',r.ID,'VILLAGE_WARD',TRUNC(SYSDATE))||chr(10))
||nvl(PKG_GLOBAL_FNTS.FN_ADDRESS('HOME',r.ID,'COUNTY',TRUNC(SYSDATE)),' ')
)]]>
