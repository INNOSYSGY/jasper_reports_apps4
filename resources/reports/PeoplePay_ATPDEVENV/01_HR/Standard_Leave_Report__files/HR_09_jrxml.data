		<queryString language="SQL">
			<![CDATA[select 
LT.LEAVE_TYPE, LT.PENDING,
(select nvl(sum(nvl(actual_days,0)),0) from HR_LV_LEAVEHISTORY LHK where  LHK.emp_id=LT.emp_id and LHK.leave_type_ID=LT.leave_type_ID and LHK.FROMDATE between to_date( $P{from_date} ,'yyyy-mm-dd' ) and to_date( $P{todate} ,'yyyy-mm-dd' ) and  (upper(pkg_global_fnts.status_descript(LHK.status))  like 'KEY%')) Keyed,
(select nvl(sum(nvl(actual_days,0)),0)  from HR_LV_LEAVEHISTORY LHK where LHK.emp_id=LT.emp_id and LHK.leave_type_ID=LT.leave_type_ID and LHK.FROMDATE between to_date( $P{from_date} ,'yyyy-mm-dd' ) and to_date( $P{todate} ,'yyyy-mm-dd' ) and  (upper(pkg_global_fnts.status_descript(LHK.status))  like 'APPR%')) Approved,
(select nvl(sum(nvl(actual_days,0)),0)  from HR_LV_LEAVEHISTORY LHK where LHK.emp_id=LT.emp_id and LHK.leave_type_ID=LT.leave_type_ID and LHK.FROMDATE between to_date( $P{from_date} ,'yyyy-mm-dd' ) and to_date( $P{todate} ,'yyyy-mm-dd' ) and  (upper(pkg_global_fnts.status_descript(LHK.status))  like 'VERI%')) Verified,
(select nvl(sum(nvl(actual_days,0)),0)  from HR_LV_LEAVEHISTORY LHK where LHK.emp_id=LT.emp_id and LHK.leave_type_ID=LT.leave_type_ID and LHK.FROMDATE between to_date( $P{from_date} ,'yyyy-mm-dd' ) and to_date( $P{todate} ,'yyyy-mm-dd' ) and  (upper(pkg_global_fnts.status_descript(LHK.status))  like 'CANCEL%')) Cancelled
from HR_LV_LEAVETRACK LT
join HR_LV_LEAVETYPE RP on RP.id=LT.LEAVE_TYPE_ID
where LT.emp_id= $P{emp_id} 
and LT.LEAVE_TYPE_ID=  case when $P{Leave_type_id} =0 then LT.LEAVE_TYPE_ID else  $P{Leave_type_id}    end
and trunc(sysdate) between trunc(lt.LV_TRACK_EFFECTIVE_DATE) and trunc(lt.END_DATE)]]>
		</queryString>
